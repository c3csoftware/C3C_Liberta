public without sharing class AcessoCallBoxService implements IAcessoCallBoxService
{
    //
    //Métodos
    //

    //Responsável por buscar as informações de login do callbox do usuário atual
    public UserCallBoxViewModel obterInformacoesDoLoginCallBoxDoUsuarioAtual()
    {
        String idUsuarioAtual = UserInfo.getUserId();
        
        //Buscando informações do usuário atual
        IUserDomain usuarioAtual = this.instanciaUserService.obterUsuarioComId(idUsuarioAtual);

        //Criando view model do Usuário call box
        UserCallBoxViewModel usuarioCallBox = (UserCallBoxViewModel) C3C_DIContainer_Injector.newInstance('UserCallBoxViewModel', usuarioAtual);

        C3C_APP_ApplicationContext.setResponse(usuarioCallBox);

        return usuarioCallBox;
    }

    //Responsável por salvar as informações login callbox
    public void salvarInformacoesAcessoCallboxUsuarioAtual(String ramalUsuario, String senhaCallbox)
    {
        String idUsuarioAtual = UserInfo.getUserId();

        IUserDomain usuarioAtual = UserCallboxDomainFactory.novaInstanciaUserDomainUsandoInformacoesCallbox(
            idUsuarioAtual,
            ramalUsuario, 
            senhaCallbox
        );

        this.instanciaUserService.upsertListaDeUsuarios(new List<IUserDomain>{usuarioAtual});
    }

    public void registrandoRamal(String ramal, String identificacaoGuia)
    {
        this.instanciaEventoL5CallboxService.acionarEventoL5CallBox('Registrando Ramal', JSON.serialize(new Map<String, Object>{
            'p0' => ramal,
            'p1' => identificacaoGuia
        }));
    }

    //
    //Atributos
    //

    private IUserService instanciaUserService
    {
        get
        {
            if(this.instanciaUserService == null)
                this.instanciaUserService = (IUserService) C3C_DIContainer_Injector.getInstance('UserService');

            return this.instanciaUserService;
        }
        set;
    }

    private IEventoL5CallboxService instanciaEventoL5CallboxService
    {
        get
        {
            if(this.instanciaEventoL5CallboxService == null)
                this.instanciaEventoL5CallboxService = (IEventoL5CallboxService) C3C_DIContainer_Injector.getInstance('EventoL5CallboxService');

            return this.instanciaEventoL5CallboxService;
        }
        set;
    }

