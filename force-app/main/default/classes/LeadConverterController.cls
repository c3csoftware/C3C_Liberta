public without sharing class LeadConverterController 
{   
    public class LeadConverterControllerException extends Exception {}

    public String tipoDeErro {get; set;}
    public String idLead {get; set;}

    public LeadConverterController(ApexPages.StandardController stdController)
    {
        Id leadId = stdController.getId();
        this.idLead = leadId;
        this.converterLeadServiceInstance.pegarLeadPorId(leadId);
    }
    
    public PageReference convertLead()
    {
        try
        {
            PageReference pageRef = this.converterLeadServiceInstance.converterLead();
            return pageRef;
        }
        catch(Exception e)
        {
            if(e.getMessage().contains('FIELD_CUSTOM_VALIDATION_EXCEPTION'))
                this.tipoDeErro = e.getMessage().split('FIELD_CUSTOM_VALIDATION_EXCEPTION,')[1].split(': \\[\\]')[0];
            else if(e.getMessage().contains('DUPLICATE_VALUE'))
                this.tipoDeErro = e.getMessage().split('DUPLICATE_VALUE,')[1];
            else if(e.getMessage().contains('CANNOT_UPDATE_CONVERTED_LEAD'))
                this.tipoDeErro = e.getMessage().split('CANNOT_UPDATE_CONVERTED_LEAD')[1];
            else
                this.tipoDeErro = e.getMessage();
            // ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.FATAL, e.getMessage()));
            return null;
        }
    }
    
    public IConverterLeadService converterLeadServiceInstance 
    {
        get
        {
            if(this.converterLeadServiceInstance == null)
                this.converterLeadServiceInstance = (IConverterLeadService) C3C_DIContainer_Injector.getInstance('ConverterLeadService');
            
            return this.converterLeadServiceInstance;
        }
        set;
    }

