    public List<AgendamentoTarefaMapper> obterAgendamentoTarefaPorPerfisCliente(List<String> listaPerfilCliente)
    {
        IAgendamentoTarefaDomain agendamentoTarefaDomain = (IAgendamentoTarefaDomain) C3C_DIContainer_Injector.newInstance('AgendamentoTarefaDomain');
        
        return (List<AgendamentoTarefaMapper>) agendamentoTarefaDomain.construirListAgendamentoTarefaMapper(this.instanciaAgendamentoTarefaSelector.obterAgendamentoTarefaPorPerfisCliente(listaPerfilCliente));
    }

    //Responsável por retornar os dados para monitorar os agendamentos de tarefa
    public MonitoramentoTarefaViewModel obterMonitoramentoAgendamentoTarefa(String idRegistro)
    {
        ContaMapper conta = this.instanciaContaService.obterContaPorId(idRegistro);

        UserMapper usuario = this.instanciaUserService.obterUsuarioComId(UserInfo.getUserId()).obterMapper();

        System.debug(usuario);

        Map<Date, DataAgendamentoTarefaViewModel> mapaDataAgendamentoTarefa = new Map<Date, DataAgendamentoTarefaViewModel>();
        Set<String> listaIdTipoTarefa = new Set<String>();
        Map<String, TaskMapper> mapaTarefaPeloTipoTarefa = new Map<String, TaskMapper>();
        Map<String, TaskMapper> mapaTarefaPeloTipoTarefaEProprietario = new Map<String, TaskMapper>();
        
        for(ITipoTarefaDomain tipoTarefa : this.instanciaTipoTarefaService.obterTipoTarefasQueNecessitamDeMonitoramento(conta.perfilCliente, usuario.idRegistro))
            listaIdTipoTarefa.add(tipoTarefa.obterMapper().recordId);

        System.debug('TipoTarefa');
        System.debug(listaIdTipoTarefa.size());

        for(TaskMapper tarefa : this.instanciaTaskService.obterTarefasAbertasComTipoTarefaIdRegistro(conta.recordId, listaIdTipoTarefa, usuario.idRegistro))
        {
            System.debug(tarefa.tipoTarefa);

            if(!mapaTarefaPeloTipoTarefa.containsKey(tarefa.agendamentoTarefaId))
                mapaTarefaPeloTipoTarefa.put(tarefa.agendamentoTarefaId, tarefa);
            //if(!mapaTarefaPeloTipoTarefaEProprietario.containsKey(tarefa.agendamentoTarefaId+'-'+tarefa.proprietario))
            //    mapaTarefaPeloTipoTarefaEProprietario.put(tarefa.agendamentoTarefaId+'-'+tarefa.proprietario, tarefa);
            if(!mapaTarefaPeloTipoTarefaEProprietario.containsKey(tarefa.agendamentoTarefaId))
                mapaTarefaPeloTipoTarefaEProprietario.put(tarefa.agendamentoTarefaId, tarefa);
        }
        
        System.debug('Tarefa');
        System.debug(mapaTarefaPeloTipoTarefa.size());

        Date dataUltimaTarefa;

        for(AgendamentoTarefaMapper agendamentoTarefa : (List<AgendamentoTarefaMapper>) ((IAgendamentoTarefaDomain) C3C_DIContainer_Injector.newInstance('AgendamentoTarefaDomain')).construirListAgendamentoTarefaMapper(this.instanciaAgendamentoTarefaSelector.buscarAgendamentosTarefaPorTiposTarefa(listaIdTipoTarefa)))
        {
            System.debug('AgendamentoTarefa');
            System.debug(agendamentoTarefa);

            TaskMapper tarefaAberta;

            Date dataReferencia = (Date) conta.record.get(agendamentoTarefa.campoDataReferente);

            System.debug('dataReferencia');
            System.debug(dataReferencia);

            if(dataReferencia == null)
                continue;

            Date dataAgendamento = dataReferencia.addDays(Integer.valueOf(agendamentoTarefa.quantidadeDias*(agendamentoTarefa.contexto == 'Depois' ? 1 : -1)));

            if(mapaTarefaPeloTipoTarefaEProprietario.containsKey(agendamentoTarefa.recordId))
            {
                tarefaAberta = mapaTarefaPeloTipoTarefaEProprietario.get(agendamentoTarefa.recordId);
                if(dataUltimaTarefa == null || tarefaAberta.dataVencimento > dataUltimaTarefa)
                    dataUltimaTarefa = tarefaAberta.dataVencimento;
                //dataAgendamento = null;
            }
            
            System.debug(agendamentoTarefa.idTipoTarefa);

            System.debug('dataAgendamento');
            System.debug(dataAgendamento);

            if(dataAgendamento != null && dataAgendamento < System.today())
                continue;

            DataAgendamentoTarefaViewModel dataAgendamentoTarefa = mapaDataAgendamentoTarefa.containsKey(dataAgendamento) ? mapaDataAgendamentoTarefa.get(dataAgendamento) : (DataAgendamentoTarefaViewModel) C3C_DIContainer_Injector.newInstance('DataAgendamentoTarefaViewModel', dataAgendamento);

            dataAgendamentoTarefa.adicionarAgendamentoTarefa(agendamentoTarefa, tarefaAberta);

            mapaDataAgendamentoTarefa.put(dataAgendamento, dataAgendamentoTarefa);
        }

        for(TaskMapper tarefa : this.instanciaTaskService.obterTarefasMesComIdRelativo(idRegistro, UserInfo.getUserId()))
        {
            Date dataReferencia = tarefa.dataVencimento;

            if(dataReferencia == null)
                continue;

            if(dataUltimaTarefa == null || dataReferencia > dataUltimaTarefa)
                dataUltimaTarefa = dataReferencia;

            DataAgendamentoTarefaViewModel dataAgendamentoTarefa = mapaDataAgendamentoTarefa.containsKey(dataReferencia) ? mapaDataAgendamentoTarefa.get(dataReferencia) : (DataAgendamentoTarefaViewModel) C3C_DIContainer_Injector.newInstance('DataAgendamentoTarefaViewModel', dataReferencia);
            
            dataAgendamentoTarefa.listaTarefasMes.add(tarefa);
            mapaDataAgendamentoTarefa.put(dataReferencia, dataAgendamentoTarefa);
        }

        for(EventMapper evento : this.instanciaEventService.obterEventosDoMesComIdRelativoIdProprietario(idRegistro, UserInfo.getUserId()))
        {
            Date dataReferencia = Date.valueOf(evento.dataDeInicio);

            if(dataReferencia == null)
                continue;

            if(dataUltimaTarefa == null || dataReferencia > dataUltimaTarefa)
                dataUltimaTarefa = dataReferencia;

            DataAgendamentoTarefaViewModel dataAgendamentoTarefa = mapaDataAgendamentoTarefa.containsKey(dataReferencia) ? mapaDataAgendamentoTarefa.get(dataReferencia) : (DataAgendamentoTarefaViewModel) C3C_DIContainer_Injector.newInstance('DataAgendamentoTarefaViewModel', dataReferencia);
            
            dataAgendamentoTarefa.compromissosMes.add(evento);
            mapaDataAgendamentoTarefa.put(dataReferencia, dataAgendamentoTarefa);
        }

        if(dataUltimaTarefa == null)
            dataUltimaTarefa = Date.valueOf(conta.dataCriacao);

        MonitoramentoTarefaViewModel monitoramentoTarefa = (MonitoramentoTarefaViewModel) C3C_DIContainer_Injector.newInstance('MonitoramentoTarefaViewModel', new Map<String, Object>{
            'listaAgendamentosTarefaCompromisso' => mapaDataAgendamentoTarefa.values(),
            'numDiasUltimaTarefa' => (dataUltimaTarefa > System.today() ? 0 : dataUltimaTarefa.daysBetween(System.today()))
        });

        C3C_APP_ApplicationContext.setResponse(monitoramentoTarefa);

        return monitoramentoTarefa;
    }

