public without sharing virtual class CasoTriggerAfterUpdateService implements ICasoTriggerAfterUpdateService
{
    //Método
    public void dispararTarefaNpsParaAssessor(List<CasoDomain> listaDeCasos)
    {
        //Variável resposável por armazenar o tipo tarefa NPS
        ITipoTarefaDomain tipoTarefaDomainCaso = this.instanciaTipoTarefaService.obterTipoTarefaComNome('NPS do Assessor');

        List<ITaskDomain> listaTarefas = new List<ITaskDomain>();

        for(CasoDomain casoDomain : listaDeCasos)
        {
            if(casoDomain.assessorQueRequisitou != null && casoDomain.status == 'Resolvido')
            {    
                //Resposável por criar tarefas de NPS relacionado ao caso     
                ITaskDomain tarefa = (ITaskDomain) C3C_DIContainer_Injector.newInstance('TaskDomain');
                TaskMapper tarefaMapper = tarefa.obterMapper();           
                
                tarefaMapper.proprietario = UserInfo.getUserId();
                tarefaMapper.assunto = 'NPS do Assessor';
                tarefaMapper.tipoTarefa = tipoTarefaDomainCaso.obterMapper().recordId;
                tarefaMapper.relativo = casoDomain.recordId;

                listaTarefas.add(tarefa);
            }
        }

        this.instanciaInserirTarefaService.criarListaDeTarefa(listaTarefas);
    }

    public IInserirTarefaService instanciaInserirTarefaService 
    {
        get
        {
            if(this.instanciaInserirTarefaService == null)
                this.instanciaInserirTarefaService = (IInserirTarefaService) C3C_DIContainer_Injector.getInstance('InserirTarefaService');
            
            return this.instanciaInserirTarefaService;
        }
        set;
    }

    public ITipoTarefaService instanciaTipoTarefaService 
    {
        get
        {
            if(this.instanciaTipoTarefaService == null)
                this.instanciaTipoTarefaService = (ITipoTarefaService) C3C_DIContainer_Injector.getInstance('TipoTarefaService');
            
            return this.instanciaTipoTarefaService;
        }
        set;
    }

