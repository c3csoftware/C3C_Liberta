public without sharing class TaskSelector extends fflib_SObjectSelector implements ITaskSelector
{
    public class TaskSelectorException extends Exception{}
    
    public Schema.SObjectType getSObjectType()
    {
        return Task.sObjectType;    
    }

    public List<Schema.SObjectField> getSObjectFieldList()
    {
        return new List<Schema.SObjectField>
        {
            Task.Id,
			Task.Subject,
			Task.OwnerId,
            Task.WhatId,
            Task.WhoId,
            Task.Status,
            Task.IsReminderSet,
            Task.TipoTarefa__c,
            Task.CompletedDateTime,
            Task.CreatedDate,
            Task.AgendamentoTarefa__c,
            Task.ActivityDate
        };
    }

    public SObject obterTarefaPorIdComPop(String idRegistro)
    {
        fflib_QueryFactory query = newQueryFactory();
        query.selectField('TipoTarefa__r.DescricaoPop__c');
        query.addCondition('', 'Id', '=', idRegistro);

        for(SObject obj : (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()))
            return obj;

        return null;
    }

    public SObject obterTarefaPorId(String idTarefa)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'Id', '=', idTarefa);

        for(SObject obj : (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()))
            return obj;

        return null; 
    }

    public List<SObject> obterTarefaPorListId(List<String> listaIdTarefa)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.selectField('TipoTarefa__r.Tipo__c');
        query.addCondition('', 'Id', 'IN', listaIdTarefa);

        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }

    public SObject obterTarefaPorTipoDeTarefaEWhatIdComLimitacao(String relativo, String nomeTipoTarefa)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        String sobjectType = ((Id) relativo).getSObjectType().getDescribe().getName();

        if(sobjectType == 'Lead')
        {
            query.addCondition('', 'WhoId', '=', relativo);
        }
        else 
        {
            query.addCondition('', 'WhatId', '=', relativo);
        }
        query.addCondition('AND', 'TipoTarefa__r.Tipo__c', '=', nomeTipoTarefa);
        query.setLimit(1);

        for(SObject obj : (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()))
            return obj;

        return null; 
    }
    
    public List<SObject> obterTaskPorListaWhatId(List<String> listaRelativo)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.selectField('TipoTarefa__r.PapelEquipe__c');
        query.selectField('TipoTarefa__r.PapelEquipe__r.Name');

        query.addCondition('', 'WhatId', 'IN', listaRelativo);
        query.addCondition('', 'Subject', '!=', '');
        query.addCondition('', 'TipoTarefa__r.Tipo__c ', '=', 'NPS');
        query.addCondition('', 'TipoTarefa__r.Tipo__c ', '=', 'Relacionamento');
        query.addCondition('', 'TipoTarefa__r.Tipo__c ', '=', 'Onboarding');
        query.setLogic('{0} AND {1} AND ({2} OR {3} OR {4})');

        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }

    public List<SObject> obterTaskPorListaWhatIdTipoTarefa(List<String> listaRelativo, List<String> tiposTarefa)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.selectField('TipoTarefa__r.PapelEquipe__c');
        query.selectField('TipoTarefa__r.PapelEquipe__r.Name');

        query.addCondition('', 'WhatId', 'IN', listaRelativo);
        query.addCondition('AND', 'Subject', '!=', '');
        query.addCondition('AND', 'TipoTarefa__r.Tipo__c ', 'IN', tiposTarefa);

        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }

    //Responsável por buscar as tarefas em aberto de acordo o registro informado e os tipos de tarefa informados.
    public List<SObject> buscarTarefasAbertasComTipoTarefaIdRelacionado(String idRegistro, Set<String> listaIdTipoTarefa, String idUsuario)
    {
        fflib_QueryFactory query = newQueryFactory(); 

        query.addCondition('', 'WhatId', '=', idRegistro);
        query.addCondition('AND', 'TipoTarefa__c', 'IN', listaIdTipoTarefa);
        query.addCondition('AND', 'Status', '=', 'Open');

        query.addOrdering('Status', fflib_QueryFactory.SortOrder.ASCENDING);
        query.addOrdering('CompletedDateTime', fflib_QueryFactory.SortOrder.DESCENDING);

        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }

    //Responsável por buscar as tarefas do mês do registro relacionado
    public List<SObject> buscarTarefasMesComIdRelativo(String idRelativo, String idProprietario)
    {
        fflib_QueryFactory query = newQueryFactory(); 

        query.addCondition('', 'WhatId', '=', idRelativo);
        query.addCondition('AND', 'OwnerId', '=', idProprietario);
        query.addCondition('AND', 'ActivityDate', '=', 'THIS_MONTH', false);

        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }

