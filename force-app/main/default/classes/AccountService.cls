public without sharing virtual class AccountService implements IAccountService
{
    public class SemCodigoAssessorException extends Exception {}

    public void porNomeEChaveEmConta(String idDaConta, LeadMapper leadMapper)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Account.SObjectType
        });

        IAccountDomain accountDomain = (IAccountDomain) C3C_DIContainer_Injector.newInstance('AccountDomain', new Map<String, Object>{
            'idRegistro' => idDaConta,
            'leadConvertido' => leadMapper
        });
        
        uow.registerDirty(accountDomain.obterRegistro());
        uow.commitWork();
    }

    public void atribuirUsuarioComoProprietarioESetarCodigo(String idDaConta, LeadMapper leadMapper)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Account.SObjectType
        });

        IAccountDomain accountDomain = (IAccountDomain) C3C_DIContainer_Injector.newInstance('AccountDomain', new Map<String, Object>{
            'idRegistro' => idDaConta,
            'leadConvertido' => leadMapper
        });

        accountDomain.obterMapper().proprietarioConta = UserInfo.getUserId();
        // Realizar um selector para pegar o usuário e despois pegar o código que está em sua conta e setar no campo abaixo.
        IUserDomain userDomain = (IUserDomain) C3C_DIContainer_Injector.newInstance('UserDomain', this.instanciaUserSelector.obterUsuarioComId(UserInfo.getUserId()));
         
        try 
        {
            String codigoAssessor = String.valueOf(userDomain.obterMapper().codigoAssessor.intValue());
            if(codigoAssessor == '')
                throw new SemCodigoAssessorException('Sem código do assessor na conversão. Error na AccountService');
            
            accountDomain.obterMapper().codigoAssessor = codigoAssessor;

            uow.registerDirty(accountDomain.obterRegistro());
            uow.commitWork();

        }catch(Exception e)
        {
            System.debug(e.getMessage());
            C3C_APP_ApplicationContext.addCriticalError('400', 'Ocorreu uma exceção ao tentar converter o Lead, err: '+e.getMessage());
        }
    }
       
    public List<IAccountDomain> obterListaContasComIds(Set<String> contaIds)
    {
        List<IAccountDomain> listaContas = new List<IAccountDomain>();

        for(SObject conta : this.instanciaAccountSelector.buscarContasPorIds(contaIds))
            listaContas.add((IAccountDomain) C3C_DIContainer_Injector.newInstance('AccountDomain', conta));

        return listaContas;
    }

    //
    //Atributos
    //

    private IAccountSelector instanciaAccountSelector
    {
        get
        {
            if(this.instanciaAccountSelector == null)
                this.instanciaAccountSelector = (IAccountSelector) C3C_DIContainer_Injector.getInstance('AccountSelector');

            return this.instanciaAccountSelector;
        }
        set;
    }
    
    private IUserSelector instanciaUserSelector
    {
        get
        {
            if(this.instanciaUserSelector == null)
                this.instanciaUserSelector = (IUserSelector) C3C_DIContainer_Injector.getInstance('UserSelector');

            return this.instanciaUserSelector;
        }
        set;
    }
    
