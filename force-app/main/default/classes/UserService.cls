public without sharing class UserService implements IUserService
{
    //Métodos
    public IUserDomain obterUsuarioComId(String idRegistro)
    {
        IUserDomain usuarioAtual = (IUserDomain) C3C_DIContainer_Injector.newInstance('UserDomain', this.instanciaUserSelector.obterUsuarioComId(idRegistro));

        return usuarioAtual;
    }

    public IUserDomain obterUsuarioPorCodigoAssessor(String codigoAssessor)
    {
        IUserDomain usuario = (IUserDomain) C3C_DIContainer_Injector.newInstance('UserDomain', this.instanciaUserSelector.obterUsuarioPorCodigoAssessor(codigoAssessor));

        return usuario;
    }

    // Responsável por obter usuarios por codigo assessor 
    public List<UserDomain> obterUsuariosPorCodigoAssessor(List<String> listaCodigoAssessor)
    {
        // List<UserDomain> usuarios = (List<UserDomain>) C3C_DIContainer_Injector.newInstance('UserDomain', (List<SObject>) this.instanciaUserSelector.obterUsuariosPorCodigoAssessor(listaCodigoAssessor));
        List<UserDomain> usuarios = UserDomainFactory.construirListaUserDomainAPartirListaSobject((List<SObject>) this.instanciaUserSelector.obterUsuariosPorCodigoAssessor(listaCodigoAssessor));

        return usuarios;
    }

    public Map<String, IUserDomain> obterMapaUsuariosPorCodigoAssessor(List<String> listaCodigoAssessor)
    {
        Map<String, IUserDomain> usuarios = UserDomainFactory.construirMapUserDomainAPartirListaSobject((List<SObject>) this.instanciaUserSelector.obterUsuariosPorCodigoAssessor(listaCodigoAssessor));

        return usuarios;
    }

    public List<IUserDomain> upsertListaDeUsuarios(List<IUserDomain> listaUsuarios)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            User.SObjectType
        });

        for(IUserDomain usuario : listaUsuarios)
            uow.registerUpsert(usuario.obterRegistro());

        uow.commitWork();

        for(IUserDomain usuario : listaUsuarios)
            usuario.preencherNovoId();

        return listaUsuarios;
    }

    //Responsável por retornar os usuários da fila informada
    public List<IUserDomain> obterListaUsuarioComIdFila(String idFila)
    {
        List<IUserDomain> listaUsuarios = new List<IUserDomain>();

        for(SObject usuario : (List<SObject>) this.instanciaUserSelector.buscarListaUsuarioComIdFila(idFila))
            listaUsuarios.add((IUserDomain) C3C_DIContainer_Injector.newInstance('UserDomain', usuario));

        return listaUsuarios;
    }

    //Responsável por retornar o usuário onde o id ActiveCampaign é igual ao informado
    public IUserDomain obterUsuarioComIdActiveCampaign(String idActiveCampaign)
    {
        return (IUserDomain) C3C_DIContainer_Injector.newInstance('UserDomain', this.instanciaUserSelector.buscarUsuarioComIdActiveCampaign(idActiveCampaign));
    }

    //Atributos
    private IUserSelector instanciaUserSelector 
    {
        get
        {
            if(this.instanciaUserSelector == null)
                this.instanciaUserSelector = (IUserSelector) C3C_DIContainer_Injector.getInstance('UserSelector');
            
            return this.instanciaUserSelector;
        }
        set;
    }
    
