global class MonitoramentoAceiteEventoJob implements Schedulable 
{
    //
    //Métodos
    //

    global void execute(SchedulableContext SC) 
    {
        List<IEventRelationDomain> listaEventosDeRoletagemQueForamRespondidos = ((IEventRelationService) C3C_DIContainer_Injector.getInstance('EventRelationService')).obterListaEventosDeRoletagemRelacionadoQueForamRespondidos();
        List<Map<String, Object>> listaDadosParaCriarTarefas = new List<Map<String, Object>>();
        //Lista tarefas recusadas/expiradas
        List<TaskMapper> listaTarefaRecusadas = new List<TaskMapper>();

        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Event.SObjectType
        });

        for(IEventRelationDomain eventRelationQueFoiAceitado : listaEventosDeRoletagemQueForamRespondidos)
        {
            EventRelationMapper eventoRelationQueFoiAceitadoMapper = eventRelationQueFoiAceitado.obterMapper();

            EventMapper evento = (EventMapper) C3C_DIContainer_Injector.newInstance('EventMapper');

            evento.idRegistro = eventoRelationQueFoiAceitadoMapper.idEvento;
            evento.eventoAceito = eventoRelationQueFoiAceitadoMapper.status == 'Accepted';
            evento.eventoNegado = eventoRelationQueFoiAceitadoMapper.status == 'Declined';

            System.debug(evento);

            if(evento.eventoAceito)
            {
                listaDadosParaCriarTarefas.add(new Map<String, Object>{
                    'idResponsavelTarefa' => eventoRelationQueFoiAceitadoMapper.idRelacao,
                    'idLead' => eventoRelationQueFoiAceitadoMapper.leadRelacionadoEvento
                });

                IConverterLeadService instanciaConvertLeadService = ((IConverterLeadService) C3C_DIContainer_Injector.getInstance('ConverterLeadService'));
                instanciaConvertLeadService.alterarProprietarioDoLeadParaAssessor(eventoRelationQueFoiAceitadoMapper.idRelacao, eventoRelationQueFoiAceitadoMapper.leadRelacionadoEvento);
            }
            else if(evento.eventoNegado)
            {
                System.debug('evento.eventoNegado');
                TaskMapper tarefa = (TaskMapper) C3C_DIContainer_Injector.newInstance('TaskMapper', new Map<String, Object>{
                    'assunto' => 'Qualificação do Assessor Recusado/Expirado',
                    'proprietario' => eventoRelationQueFoiAceitadoMapper.proprietarioRelacionadoEvento,
                    'relativo' => null,
                    'tipoTarefa' => null
                });

                tarefa.whoId = eventoRelationQueFoiAceitadoMapper.leadRelacionadoEvento;

                listaTarefaRecusadas.add(tarefa);
            }

            uow.registerDirty(evento.record);
        }

        ((ITarefaConversaoLeadService) C3C_DIContainer_Injector.getInstance('TarefaConversaoLeadService')).criarListaTarefasConversaoLead(listaDadosParaCriarTarefas);
        ((ITaskService) C3C_DIContainer_Injector.getInstance('TaskService')).inserirListaTarefa(listaTarefaRecusadas);

        uow.commitWork();

        if(SC != null)
        {
            System.abortJob(SC.getTriggerId());
        }

        //Reagendar Job
        Datetime agendamento = System.now().addMinutes(5);
        Schedulable monitoramentoAceiteEventoJob = (Schedulable) C3C_DIContainer_Injector.newInstance('MonitoramentoAceiteEventoJob');
        System.schedule('Monitoramento aceite evento de roletagem', '0 '+agendamento.minute()+' '+agendamento.hour()+' * * ?', monitoramentoAceiteEventoJob);
    }

    //
    //Atributos - Serviços
    //

    private IEventRelationService instanciaEventRelationService
    {
        get
        {
            if(this.instanciaEventRelationService == null)
                this.instanciaEventRelationService = (IEventRelationService) C3C_DIContainer_Injector.getInstance('EventRelationService');

            return this.instanciaEventRelationService;
        }
        set;
    }

    private ITarefaConversaoLeadService instanciaTarefaConversaoLeadService
    {
        get
        {
            if(this.instanciaTarefaConversaoLeadService == null)
                this.instanciaTarefaConversaoLeadService = (ITarefaConversaoLeadService) C3C_DIContainer_Injector.getInstance('TarefaConversaoLeadService');
            
                return this.instanciaTarefaConversaoLeadService;
        }
        set;
    }


