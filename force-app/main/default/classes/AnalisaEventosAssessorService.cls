public without sharing class AnalisaEventosAssessorService implements IAnalisaEventosAssessorService
{
    public void criarTarefaParaHunter(List<IEventRelationDomain> listaEventRelation)
    {
        List<IEventRelationDomain> listaEventRelationStatusNew = new List<IEventRelationDomain>();
        List<IEventDomain> listaEventDomain = new List<IEventDomain>();
        this.filtrarEGerarListasEventRelation(listaEventRelation, listaEventRelationStatusNew);
        List<TaskMapper> listaTarefas = new List<TaskMapper>();
        for(IEventRelationDomain eventRelation : listaEventRelation)
        {
            EventRelationMapper eventRelationMapp = eventRelation.obterMapper();
            Datetime dataAgora = Datetime.now();
            Decimal calcHoras = Decimal.valueof((dataAgora.getTime() - eventRelationMapp.dataCriacao.getTime()))/(60*60);
            Decimal horas = (calcHoras/1000);

            if(eventRelationMapp.status == 'New' && horas >= 1)
            {
                listaTarefas.add(TaskMapperFactory.construirMapper(
                    'Qualificação do Assessor Recusado/Expirado - ' + eventRelationMapp.nomeLeadRelacionadoEvento + ' ' + eventRelationMapp.sobrenomeLeadRelacionadoEvento, 
                    eventRelationMapp.proprietarioRelacionadoEvento, 
                    eventRelationMapp.leadRelacionadoEvento,
                    ''
                ));
                listaEventDomain.add((IEventDomain) C3C_DIContainer_Injector.newInstance('EventDomain', new Map<String,Object>{
                    'idRegistro' => eventRelationMapp.idEvento,
                    'analisadoBatch' => true
                }));
            }
        }
        this.instanciaAtualizarEventoService.atualizaAnalisadoBatchDeEvento(listaEventDomain);
        this.instanciaTaskService.inserirListaTarefa(listaTarefas);
        this.instanceEventRelationService.excluirRelacaoDoEvento(listaEventRelationStatusNew);
    }

    private void filtrarEGerarListasEventRelation(List<IEventRelationDomain> listaEventRelation, List<IEventRelationDomain> listaEventRelationStatusNew)
    {
        for(IEventRelationDomain eventRelation : listaEventRelation)
        {
            EventRelationMapper eventRelationMapp = eventRelation.obterMapper();
            Datetime dataAgora = Datetime.now();
            Decimal calcHoras = Decimal.valueof((dataAgora.getTime() - eventRelationMapp.dataCriacao.getTime()))/(60*60);
            Decimal horas = (calcHoras/1000);
            if(eventRelationMapp.status == 'New' && horas >= 1)
                listaEventRelationStatusNew.add(eventRelation);
        }
    }

    //Serviços
    public IEventRelationService instanceEventRelationService 
    {
        get
        {
            if(this.instanceEventRelationService == null)
                this.instanceEventRelationService = (IEventRelationService) C3C_DIContainer_Injector.getInstance('EventRelationService');
            
            return this.instanceEventRelationService;
        }
        set;
    }

    private ITaskService instanciaTaskService
    {
        get
        {
            if(this.instanciaTaskService == null)
                this.instanciaTaskService = (ITaskService) C3C_DIContainer_Injector.getInstance('TaskService');

            return this.instanciaTaskService;
        }
        set;
    }

    private IAtualizarEventoService instanciaAtualizarEventoService
    {
        get
        {
            if(this.instanciaAtualizarEventoService == null)
                this.instanciaAtualizarEventoService = (IAtualizarEventoService) C3C_DIContainer_Injector.getInstance('AtualizarEventoService');

            return this.instanciaAtualizarEventoService;
        }
        set;
    }


