public with sharing class AgendamentoTarefaSelector extends fflib_SObjectSelector implements IAgendamentoTarefaSelector
{
    public class AccountTeamMemberSelectorException extends Exception{}

    public Schema.SObjectType getSObjectType()
    {
        return AgendamentoTarefa__c.sObjectType;    
    }

    public List<Schema.SObjectField> getSObjectFieldList()
    {
        return new List<Schema.SObjectField>
        {
            AgendamentoTarefa__c.Id,
			AgendamentoTarefa__c.Name,
			AgendamentoTarefa__c.TipoTarefa__c,
			AgendamentoTarefa__c.QuantidadeDias__c,
            AgendamentoTarefa__c.CreatedDate,
            AgendamentoTarefa__c.CampoDataReferente__c,
            AgendamentoTarefa__c.Repetitivo__c,
            AgendamentoTarefa__c.Contexto__c
        };
    }

    public List<SObject> obterAgendamentoTarefaPorPerfisCliente(List<String> listaPerfilCliente)
    {
        fflib_QueryFactory query = newQueryFactory();
        query.selectField('TipoTarefa__r.Tipo__c');
        query.selectField('TipoTarefa__r.PerfilCliente__c');
        query.selectField('TipoTarefa__r.PapelEquipe__r.Name');
        query.addCondition('', 'TipoTarefa__r.PerfilCliente__c', 'IN', listaPerfilCliente);

        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }

    //Respons√°vel por buscar os agendamentos relacionados aos tipos de tarefa
    public List<SObject> buscarAgendamentosTarefaPorTiposTarefa(Set<String> listaIdTipoTarefa)
    {
        fflib_QueryFactory query = newQueryFactory();
        query.addCondition('', 'TipoTarefa__c', 'IN', listaIdTipoTarefa);
        query.selectField('TipoTarefa__r.Tipo__c');

        query.setOrdering('QuantidadeDias__c', fflib_QueryFactory.SortOrder.ASCENDING);

        System.debug(query.toSOQL());

        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }
    
