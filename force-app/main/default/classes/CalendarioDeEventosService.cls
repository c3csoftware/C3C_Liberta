public without sharing class CalendarioDeEventosService implements ICalendarioDeEventosService
{
    //Método resposável por retornar a agenda de eventos de todos os usuário da fila.
    public List<AgendaDoUsuarioViewModel> obterAgendaDeUsuariosDaFilaDurantePeriodo(String idFila, Datetime dataHoraInicioPeriodo, Datetime dataHoraTerminoPeriodo)
    {
        Map<String, List<EventMapper>> mapaEventosPorUsuario = new Map<String, List<EventMapper>>();
        List<AgendaDoUsuarioViewModel> listaAgendaDosUsuarios = new List<AgendaDoUsuarioViewModel>();

        for(IEventDomain eventoUsuario : this.instanciaEventService.buscarEventosDeUsuariosDaFilaComDataDeInicioETermino(idFila, dataHoraInicioPeriodo, dataHoraTerminoPeriodo))
        {
            EventMapper eventoUsuarioMapper = eventoUsuario.obterMapper();
            List<EventMapper> listaEventosUsuario = mapaEventosPorUsuario.containsKey(eventoUsuarioMapper.proprietario) ? mapaEventosPorUsuario.get(eventoUsuarioMapper.proprietario) : new List<EventMapper>();
            listaEventosUsuario.add(eventoUsuarioMapper);

            mapaEventosPorUsuario.put(eventoUsuarioMapper.proprietario, listaEventosUsuario);
        }

        for(IMembroFilaDomain membroFila : this.instanciaMembroFilaService.obterMembrosFilaPorIdFila(idFila))
        {
            MembroFilaMapper membroFilaMapper = membroFila.obterMapper();
            List<EventMapper> listaEventosUsuario = mapaEventosPorUsuario.containsKey(membroFilaMapper.usuarioRelacionado) ? mapaEventosPorUsuario.get(membroFilaMapper.usuarioRelacionado) : new List<EventMapper>();
            
            listaAgendaDosUsuarios.add((AgendaDoUsuarioViewModel) C3C_DIContainer_Injector.newInstance('AgendaDoUsuarioViewModel', new Map<String, Object>{
                'idUsuario' => membroFilaMapper.usuarioRelacionado,
                'eventos' => listaEventosUsuario
            }));
        }

        C3C_APP_ApplicationContext.setResponse(listaAgendaDosUsuarios);

        return listaAgendaDosUsuarios;
    }

    //Método resposável por retornar a agenda de eventos de todos os usuários no periodo informado.
    public Map<String, List<EventMapper>> obterAgendaDeUsuariosDaFilaSemUsuariosInformadosDurantePeriodo(String idFila, Set<String> idsUsuario, Datetime dataHoraInicioPeriodo, Datetime dataHoraTerminoPeriodo)
    {
        Map<String, List<EventMapper>> mapaEventosPorUsuario = new Map<String, List<EventMapper>>();

        for(IEventDomain eventoUsuario : this.instanciaEventService.buscarEventosDeUsuariosDaFilaSemUsuariosInformadosComDataDeInicioETermino(idFila, idsUsuario, dataHoraInicioPeriodo, dataHoraTerminoPeriodo))
        {
            EventMapper eventoUsuarioMapper = eventoUsuario.obterMapper();
            List<EventMapper> listaEventosUsuario = mapaEventosPorUsuario.containsKey(eventoUsuarioMapper.proprietario) ? mapaEventosPorUsuario.get(eventoUsuarioMapper.proprietario) : new List<EventMapper>();
            listaEventosUsuario.add(eventoUsuarioMapper);

            mapaEventosPorUsuario.put(eventoUsuarioMapper.proprietario, listaEventosUsuario);
        }

        return mapaEventosPorUsuario;
    }

    //Atributos de serviços
    private IEventService instanciaEventService
    {
        get
        {
            if(this.instanciaEventService == null)
                this.instanciaEventService = (IEventService) C3C_DIContainer_Injector.getInstance('EventService');

            return this.instanciaEventService;
        }
        set;
    }

    private IMembroFilaService instanciaMembroFilaService
    {
        get
        {
            if(this.instanciaMembroFilaService == null)
                this.instanciaMembroFilaService = (IMembroFilaService) C3C_DIContainer_Injector.getInstance('MembroFilaService');

            return this.instanciaMembroFilaService;
        }
        set;
    }

