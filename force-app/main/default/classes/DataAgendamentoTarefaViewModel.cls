public without sharing class DataAgendamentoTarefaViewModel implements C3C_DI_IProvider 
{
    //
    //Atributos
    //

    @AuraEnabled public Date dataAgendamento {get;set;}
    @AuraEnabled 
    public String dataAgendamentoFormatado 
    {
        get
        {
            if(this.dataAgendamento != null)
                this.dataAgendamentoFormatado = converterDateParaString(dataAgendamento);
            else
                this.dataAgendamentoFormatado = '---';

            return this.dataAgendamentoFormatado;
        }
        set;
    }
    @AuraEnabled public List<AgendamentoTarefaViewModel> listaAgendamentoTarefa 
    {
        get
        {
            if(this.listaAgendamentoTarefa == null)
                this.listaAgendamentoTarefa = new List<AgendamentoTarefaViewModel>();

            return this.listaAgendamentoTarefa;
        }
        set;
    }

    @AuraEnabled public List<TaskMapper> listaTarefasMes = new List<TaskMapper>();
    @AuraEnabled public List<EventMapper> compromissosMes = new List<EventMapper>();

    //
    //Construtores
    //

    public DataAgendamentoTarefaViewModel()
    {}

    //Responsável por criar a instância passando dataAgendamento e informacoes de tarefa
    private DataAgendamentoTarefaViewModel(Date dataAgendamento)
    {
        this.dataAgendamento = dataAgendamento;
    }

    //
    //Métodos
    //
    
    public Object newInstance(Object params)
    {
        if(params instanceOf Map<String, Object>)
        {
            Map<String, Object> mapaParams = (Map<String, Object>) params;

            return new DataAgendamentoTarefaViewModel(
                (Date) mapaParams.get('dataAgendamento')
            );
        }

        if(params instanceOf Date)
            return new DataAgendamentoTarefaViewModel((Date) params);

        return null;
    }

    public void adicionarAgendamentoTarefa(AgendamentoTarefaMapper agendamento, TaskMapper tarefaAberta)
    {
        this.listaAgendamentoTarefa.add(new AgendamentoTarefaViewModel(agendamento, tarefaAberta));
    }

    private String converterDateParaString(Date dataLigacao)
    {
        if(dataLigacao == null)
            return null;

        Integer dia = dataLigacao.day();
        Integer mes = dataLigacao.month();
        Integer ano = dataLigacao.year();

        return this.formatarNumero(dia)+'/'+this.formatarNumero(mes)+'/'+ano;
    }

    private String formatarNumero(Integer numero)
    {
        return numero < 10 ? '0'+numero : String.valueOf(numero);
    }

    public class AgendamentoTarefaViewModel
    {
        @AuraEnabled public TaskMapper tarefaAberta {get;set;}
        @AuraEnabled public AgendamentoTarefaMapper listaAgendamentoTarefa {get;set;}

        public AgendamentoTarefaViewModel(AgendamentoTarefaMapper listaAgendamentoTarefa, TaskMapper tarefaAberta)
        {
            this.tarefaAberta = tarefaAberta;
            this.listaAgendamentoTarefa = listaAgendamentoTarefa;
        }
    }

