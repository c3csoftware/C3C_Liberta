public without sharing class TaskDomain implements ITaskDomain, C3C_DI_IProvider
{
    public class TaskDomainException extends Exception {}

    private TaskMapper taskMapper 
    {
        get
        {
            if(this.taskMapper == null)
                this.taskMapper = (TaskMapper) C3C_DIContainer_Injector.newInstance('TaskMapper');

            return this.taskMapper;
        }
        set;
    }
    public List<TaskMapper> listTaskMapper {get; set;}

    //Contrutores
    public TaskDomain() 
    {}
    
    private TaskDomain(SObject taskSObject)
    {
        this.construirDomainComSObject(taskSObject);
    }

    private TaskDomain(String idTask, String status)
    {
        this.construirDomainComIdEStatus(idTask, status);
    }
    
    private TaskDomain(String idTask, String whoId, String relativo)
    {
        this.construirDomainComIdEWhoId(idTask, whoId, relativo);
    }

    //Métodos
    public Object newInstance(Object params)
    {
        if(params instanceOf List<SObject>)
            return this.construirListaDomainComListaSObject((List<SObject>) params);
        
        if(params instanceOf SObject)
            return new TaskDomain((SObject) params);
        
        if(params instanceOf Map<String,Object>)
        {
            Map<String,Object> mapaParams = (Map<String,Object>) params;
            if(mapaParams.containsKey('status'))
                return new TaskDomain((String) mapaParams.get('idTask'), (String) mapaParams.get('status'));
            else
                return new TaskDomain((String) mapaParams.get('idTask'), (String) mapaParams.get('whoId'), (String) mapaParams.get('relativo'));
        }

        return null;
    }

    //Responsável por retornar a mapper
    public TaskMapper obterMapper()
    {
        return this.taskMapper;
    }

    //Responsável por retornar o registro SObject
    public SObject obterRegistro()
    {
        return this.taskMapper.record;
    }

    //Responsável por construir a domain com sobject
    private void construirDomainComSObject(SObject taskSObject)
    {
        this.taskMapper = (TaskMapper) C3C_DIContainer_Injector.newInstance('TaskMapper', taskSObject);
    }

    //Responsável por construir uma lista de domain com uma lista de sobject
    private List<TaskDomain> construirListaDomainComListaSObject(List<SObject> listaUsuariosSObject)
    {
        List<TaskDomain> listaDeUsuariosDomain = new List<TaskDomain>();

        for(SObject taskSObject : listaUsuariosSObject)
            listaDeUsuariosDomain.add(new TaskDomain(taskSObject));

        return listaDeUsuariosDomain;
    }

    //Responsável por construir a domain com idTask e status
    private void construirDomainComIdEStatus(String idTask, String status)
    {
        this.taskMapper = (TaskMapper) C3C_DIContainer_Injector.newInstance('TaskMapper', new Map<String,Object>{
            'idTask' => idTask,
            'status' => status
        });
    }
    
    //Responsável por construir a domain com idTask, whoId e sem whatId
    private void construirDomainComIdEWhoId(String idTask, String whoId, String relativo)
    {
        this.taskMapper = (TaskMapper) C3C_DIContainer_Injector.newInstance('TaskMapper', new Map<String,Object>{
            'idTask' => idTask,
            'whoId' => whoId,
            'relativo' => relativo
        });
    }

    public TaskMapper criaTaskMapper(String assunto, String proprietario, String relativo, String tipoTarefa)
    {
        Datetime myDate = Datetime.now();
        this.taskMapper = (TaskMapper) C3C_DIContainer_Injector.newInstance('TaskMapper', new Map<String,Object>{
            'assunto' => assunto,
            'proprietario' => proprietario,
            'relativo' => relativo,
            'tipoTarefa' => tipoTarefa
        });
        
        return this.taskMapper; 
    }

    public TaskMapper construirTaskMapper(SObject task)
    {
        this.taskMapper = (TaskMapper) C3C_DIContainer_Injector.newInstance('TaskMapper', task);
        
        return this.taskMapper;
    }

    public List<TaskMapper> construirListTaskMapper(List<SObject> listTask)
    {
        this.listTaskMapper = (List<TaskMapper>) C3C_DIContainer_Injector.newInstance('TaskMapper', listTask);
        
        return this.listTaskMapper;
    }
    
