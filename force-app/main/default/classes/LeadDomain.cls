public without sharing class LeadDomain implements ILeadDomain, C3C_DI_IProvider
{
    public class LeadDomainException extends Exception {}

    public LeadMapper leadMapper {get; set;}
    public List<LeadMapper> listLeadMapper {get; set;}
    public Map<String, LeadMapper> mapLeadMapper {get; set;}

    //Contrutores
    public LeadDomain() 
    {}
    
    private LeadDomain(SObject leadSObject)
    {
        this.construirDomainComSObject(leadSObject);
    }

    private LeadDomain(LeadMapper leadMpp)
    {
        this.leadMapper = leadMpp;
    }
    
    private LeadDomain(String idLead, String status)
    {
        this.construirDomainComIdEStatus(idLead, status);
    }
    
    private LeadDomain(Id idRegistro, Id idProprietario)
    {
        this.construirDomainComIdEProprietario(idRegistro, idProprietario);
    }

    // Métodos 

    public Object newInstance(Object params)
    {
        if(params instanceOf List<SObject>)
            return this.construirListaDomainComListaSObject((List<SObject>) params);
            
        if(params instanceOf SObject)
            return new LeadDomain((SObject) params);

        if(params instanceOf Map<String,Object>)
        {
            Map<String,Object> mapaParams = (Map<String,Object>) params;
            if(mapaParams.containsKey('idLead'))
                return new LeadDomain((String) mapaParams.get('idLead'), (String) mapaParams.get('status'));

            return new LeadDomain((Id) mapaParams.get('idRegistro'), (Id) mapaParams.get('idProprietario'));
        }

        return null;
    }

    //Responsável por retornar a mapper
    public LeadMapper obterMapper()
    {
        return this.leadMapper;
    }

    //Responsável por retornar o registro SObject
    public SObject obterRegistro()
    {
        return this.leadMapper.record;
    }

    public LeadMapper criarLead(String activeCampaignId, String nome, String sobrenome, String email, String empresa, String telefone)
    {
        this.leadMapper = (LeadMapper) C3C_DIContainer_Injector.newInstance('LeadMapper', new Map<String, Object>{
            'activeCampaignId' => activeCampaignId,
            'nome' => nome,
            'sobrenome' => sobrenome,
            'email' => email,
            'empresa' => empresa,
            'telefone' => telefone
        });
        
        return this.leadMapper;
    }

    public LeadMapper construirLeadMapper(SObject lead)
    {
        this.leadMapper = (LeadMapper) C3C_DIContainer_Injector.newInstance('LeadMapper', lead);
        
        return this.leadMapper;
    }

    public List<LeadMapper> construirListLeadMapper(List<SObject> listLead)
    {
        this.listLeadMapper = (List<LeadMapper>) C3C_DIContainer_Injector.newInstance('LeadMapper', listLead);
        
        return this.listLeadMapper;
    }

    public Map<String, LeadMapper> construirMapLeadMapper(List<SObject> listLead)
    {
        List<LeadMapper> listLeadMapperParaMap = (List<LeadMapper>) C3C_DIContainer_Injector.newInstance('LeadMapper', listLead);
        this.mapLeadMapper = new Map<String, LeadMapper>{};
        for(LeadMapper leadMapper : listLeadMapperParaMap)
        {
            System.debug('leadMapper.recordId:'+leadMapper.recordId);
            this.mapLeadMapper.put(leadMapper.recordId, leadMapper);
        }
        return this.mapLeadMapper;
    }

    //Responsável por construir a domain com idLead e status
    private void construirDomainComIdEStatus(String idLead, String status)
    {
        this.leadMapper = (LeadMapper) C3C_DIContainer_Injector.newInstance('LeadMapper', new Map<String,Object>{
            'idLead' => idLead,
            'status' => status
        });
    }
    
    //Responsável por construir a domain com idRegistro e idProprietario
    private void construirDomainComIdEProprietario(Id idRegistro, Id idProprietario)
    {
        this.leadMapper = (LeadMapper) C3C_DIContainer_Injector.newInstance('LeadMapper', new Map<String,Object>{
            'idRegistro' => idRegistro,
            'idProprietario' => idProprietario
        });
    }
    
    //Responsável por construir a domain com sobject
    private void construirDomainComSObject(SObject leadSObject)
    {
        this.leadMapper = (LeadMapper) C3C_DIContainer_Injector.newInstance('LeadMapper', leadSObject);
    }

    //Responsável por construir uma lista de domain com uma lista de sobject
    private List<LeadDomain> construirListaDomainComListaSObject(List<SObject> listaLeadsSObject)
    {
        List<LeadDomain> listaDeLeadsDomain = new List<LeadDomain>();

        for(SObject leadSObject : listaLeadsSObject)
            listaDeLeadsDomain.add(new LeadDomain(leadSObject));

        return listaDeLeadsDomain;
    }
    
