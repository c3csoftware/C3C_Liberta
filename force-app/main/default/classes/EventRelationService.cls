public without sharing class EventRelationService implements IEventRelationService
{
    //Exeptions 
    public class EventRelationServiceException extends Exception {}

    //Métodos
    //Método responsável por criar relacaoDoEvento.
    public IEventRelationDomain criarRelacaoDoEvento(String idEvento, String idRelacao)
    {
        IEventRelationDomain eventRelationDomain = (IEventRelationDomain) C3C_DIContainer_Injector.newInstance('EventRelationDomain', new Map<String, Object>{
            'idEvento' => idEvento,
            'idRelacao' => idRelacao
        });
        
        try 
        {
            Database.DMLOptions dmo = new Database.DMLOptions(); 
            dmo.EmailHeader.triggerUserEmail = true; 
            Database.insert(eventRelationDomain.obterRegistro(), dmo);

            eventRelationDomain.atribuirNovoIdDoRegistro();

            C3C_APP_ApplicationContext.setResponse(eventRelationDomain.obterMapper());
            return eventRelationDomain;
        } 
        catch (Exception e) 
        {
            throw new EventRelationServiceException('400 - Ocorreu uma exceção ao tentar salvar a Relação do Evento, err: '+e.getMessage());
        }
    }
    
    //Método responsável por deletar relacaoDoEvento.
    public void excluirRelacaoDoEvento(List<IEventRelationDomain> listaRelacaoDoEvento)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            EventRelation.SObjectType
        });
        
        for(IEventRelationDomain relacaoDoEvento : listaRelacaoDoEvento)
        {
            uow.registerDeleted(relacaoDoEvento.obterRegistro());
        }

        try 
        {
            uow.commitWork();
        } 
        catch (Exception e) 
        {
            throw new EventRelationServiceException('400 - Ocorreu uma exceção ao tentar excluir a Relação do Evento, err: '+e.getMessage());
        }
    }

    //Método responsável por retornar a lista de eventos de roletagem relacionados, que acabaram de ser aceitos
    public List<IEventRelationDomain> obterListaEventosDeRoletagemRelacionadoQueForamRespondidos()
    {
        List<IEventRelationDomain> listaEventosDeRoletagem = new List<IEventRelationDomain>();

        for(SObject eventoObj : this.instanciaEventRelationSelector.buscarEventosDeRoletagemRelacionadoQueForamRespondidos())
        {
            listaEventosDeRoletagem.add((IEventRelationDomain) C3C_DIContainer_Injector.newInstance('EventRelationDomain', eventoObj));
        }

        return listaEventosDeRoletagem;
    }

    //Atributos - Selector

    private IEventRelationSelector instanciaEventRelationSelector
    {
        get
        {
            if(this.instanciaEventRelationSelector == null)
                this.instanciaEventRelationSelector = (IEventRelationSelector) C3C_DIContainer_Injector.getInstance('EventRelationSelector');

            return this.instanciaEventRelationSelector;
        }
        set;
    }
    
