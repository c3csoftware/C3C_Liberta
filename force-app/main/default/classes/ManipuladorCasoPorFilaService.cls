public without sharing virtual class ManipuladorCasoPorFilaService implements IManipuladorCasoPorFilaService
{
    // Exceptions
    private class ManipuladorCasoPorFilaServiceException extends Exception {}

    // Método responsável por definir qual fila pertence tal caso passado na trigger 
    public void orquestrarFilaParaDefinirProprietarioCaso(List<CasoDomain> casosDomain)
    {
        List<CasoDomain> listaCasosDomainBackoffice = new List<CasoDomain>();
        List<CasoDomain> listaCasosDomainTrader = new List<CasoDomain>();

        for (CasoDomain casoDomain : casosDomain)
        {   
            switch on casoDomain.categoria
            {
                when 'Backoffice'
                {
                    listaCasosDomainBackoffice.add(casoDomain);
                }
                when 'Trader'
                {
                    listaCasosDomainTrader.add(casoDomain);
                }
                when else
                {
                    throw new ManipuladorCasoPorFilaServiceException('Categoria informada não está definida, entre em contato com o adminstrador do sistema');
                }
            }
        }

        // Se a lista de casos para Backoffice estiver preenchida, então será executada a lógica de manipulação dos casos
        if (listaCasosDomainBackoffice.size() > 0)
        {
            this.manipularCasosBackOffice(listaCasosDomainBackoffice);
        }

        // Se a lista de casos para Trader estiver preenchida, então será executada a lógica de manipulaçao dos casos.
        if (listaCasosDomainTrader.size() > 0)
        {
            Map<String, GroupMapper> mapGroupMapperPorNomeFila = new Map<String, GroupMapper>();
            List<MembroEquipeMapper> listaMembroEquipeMapper = new List<MembroEquipeMapper>();

            // Fazendo a busca das filas com Tag inicio [Trader];
            List<GroupMapper> listaGroupMapper = this.instanciaGroupService.obterGroupPorTagInicio('[Trader]');

            // Pegando a domain de Usuário passando o Usuário do contexto.
            IUserDomain usuarioContexto = this.instanciaUserService.obterUsuarioComId(UserInfo.getUserId());

            // Populando a Lista de Membro Equipe relacionado ao usuário do contexto.
            for (SObject registro : this.instanciaMembroEquipeSelector.obterMembroEquipePorUsuarioId(usuarioContexto.obterMapper().idRegistro))
            {
                listaMembroEquipeMapper.add( (MembroEquipeMapper) C3C_DIContainer_Injector.newInstance('MembroEquipeMapper', registro) );
            }
            
            // Populando o Map de GroupMapperPorNomeFila.
            for (GroupMapper groupMapper : listaGroupMapper)
            {
                mapGroupMapperPorNomeFila.put(groupMapper.nome, groupMapper);
            }

            // Pegando o papel do usuário do contexto.
            String papelUsuarioContexto = usuarioContexto.obterMapper().papel;
            
            // Realizando a lógica de manipular casos para o tipo Trader.
            for (CasoDomain caso : listaCasosDomainTrader)
            {
                caso.proprietario = caso.definirProprietarioCasoPorFilaCategoriaTrader(papelUsuarioContexto, listaMembroEquipeMapper, mapGroupMapperPorNomeFila);
            }
        }

    }

    private void manipularCasosBackOffice(List<CasoDomain> listaCasosDomain)
    {
        List<GroupMapper> listaGroupMapper = this.instanciaGroupService.obterGroupPorTagInicio('[Backoffice]');

            for (GroupMapper groupMapper : listaGroupMapper)
            {
                for (CasoDomain casoDomain: listaCasosDomain )
                {
                    if (casoDomain.subCategoria.contains(groupMapper.nome.split('] ')[1]))
                    {
                        casoDomain.proprietario = groupMapper.recordId;
                    }
                }
            }
    }

    // Instâncias

    private IGroupService instanciaGroupService
    {
        get
        {
            if (this.instanciaGroupService == null)
                this.instanciaGroupService = (IGroupService) C3C_DIContainer_Injector.getInstance('GroupService');

            return this.instanciaGroupService;
        }
        set;
    }

    private IUserService instanciaUserService
    {
        get
        {
            if (this.instanciaUserService == null)
                this.instanciaUserService = (IUserService) C3C_DIContainer_Injector.getInstance('UserService');

            return this.instanciaUserService;
        }
        set;
    }

    private IMembroEquipeSelector instanciaMembroEquipeSelector
    {
        get
        {
            if (this.instanciaMembroEquipeSelector == null)
                this.instanciaMembroEquipeSelector = (IMembroEquipeSelector) C3C_DIContainer_Injector.getInstance('MembroEquipeSelector');

            return this.instanciaMembroEquipeSelector;
        }
        set;
    }

