public without sharing class ListaActiveCampaignDomain implements IListaActiveCampaignDomain
{
    public class ListaActiveCampaignDomainException extends Exception {}
    
    public ListaActiveCampaignMapper listaActiveCampaignMapper {get; set;}
    public List<ListaActiveCampaignMapper> listListaActiveCampaignMapper {get; set;}

    public List<ListaActiveCampaignMapper> construirListListaActiveCampaignMapper(List<SObject> listListaActiveCampaign)
    {
        this.listListaActiveCampaignMapper = (List<ListaActiveCampaignMapper>) C3C_DIContainer_Injector.newInstance('ListaActiveCampaignMapper', listListaActiveCampaign);
        
        return this.listListaActiveCampaignMapper;
    }
    
    public ListaActiveCampaignMapper criarListaActiveCampaignPorIdLista(String idLista, String nomeLista, String nomeFila)
    {
        this.listaActiveCampaignMapper = (ListaActiveCampaignMapper) C3C_DIContainer_Injector.newInstance('ListaActiveCampaignMapper', new Map<String, Object>{
           	 'idLista' => idLista,
             'nomeLista' => nomeLista,
             'nomeFila' => nomeFila
        });
        
        return this.listaActiveCampaignMapper;
    }

    public String verificarNomeListaLibertaOuLS(String nomeLista)
    {
        if (nomeLista.contains('[LS]') || nomeLista.contains('[SF]'))
        {
            if (nomeLista.contains('[LS]'))
                return 'LS';

            return 'SF';
        }

        return 'NÃ£o possui Fila';
    }

    public String tratarNomeListaActiveCampaign(String nomeLista)
    {
        String novoNomeLista;
        String nomeListaTemporario;

        if(nomeLista.contains('[') && nomeLista.contains(']'))
        {
            if (nomeLista.countMatches('[') == nomeLista.countMatches(']'))
            {
                novoNomeLista = '['+nomeLista.substringBetween('[', ']')+']';
                if (nomeLista.countMatches('[') > 1)
                {
                    nomeLista = nomeLista.removeStart(novoNomeLista);
                    for(Integer i = 0; i <= nomeLista.countMatches('['); i++)
                    {
                        nomeListaTemporario = '['+nomeLista.substringBetween('[', ']')+']';
                        novoNomeLista += nomeListaTemporario;
                        nomeLista = nomeLista.removeStart(nomeListaTemporario);
                    }
                }
    
                if(novoNomeLista.contains('[LS]'))
                    return 'Comercial';

                if(novoNomeLista.contains('[Liberta]'))
                    return 'Hunter';

                return 'Sem TAG no Active';
            }
        }
        return 'Sem TAG no Active';
    }
    
