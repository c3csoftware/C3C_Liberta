public without sharing class AccountTriggerService implements IAccountTriggerService
{
    public void atribuirProdutoLsEmConta(List<ContaMapper> listaContaMapper)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{Pedido__c.SObjectType});

        List<String> listaIdChaveExterna = new List<String>();
        Map<String, ContaMapper> mapaContaMapperPorIdChaveExterna = new Map<String, ContaMapper>();

        for (ContaMapper conta : listaContaMapper)
        {
            String idChaveExterna = conta.idChaveExterna;

            listaIdChaveExterna.add(idChaveExterna);
            mapaContaMapperPorIdChaveExterna.put(idChaveExterna, conta);
        }

        List<SObject> listaProdutosLs = [SELECT Id, Name, Nome__c, CpfCnpj__c, Conta__c, DataPagamento__c,Email__c, Telefone__c
                                        FROM Pedido__c WHERE CpfCnpj__c IN :listaIdChaveExterna AND Conta__c = null];
        
        List<PedidoDomain> listaPedidoDomain = PedidoDomainFactory.construirListaPedidoDomainAPartirListaSobject(listaProdutosLs);

        for (PedidoDomain pedidoDomain : listaPedidoDomain)
        {

            if (mapaContaMapperPorIdChaveExterna.get(pedidoDomain.cpfCnpj) != null)
            {
                pedidoDomain.conta = mapaContaMapperPorIdChaveExterna.get(pedidoDomain.cpfCnpj).recordId;
                uow.registerDirty(pedidoDomain.record);
            }
        }

        uow.commitWork();
    }
    
    public void atribuirNomeConta(List<ContaMapper> listaContaMapper)
    {
        for (ContaMapper conta : listaContaMapper)
        {
            if (String.isBlank(conta.nome))
                conta.nome = '⠀';
        }
    }
    
    public void atribuirPerfilClienteNaConta(List<ContaMapper> listaContaMapper)
    {
        List<PerfilClienteMapper> listaPerfilClienteMapper = this.perfilClienteServiceInstance.obterPerfilClientes();
        
        for (ContaMapper conta : listaContaMapper)
        {
            if (conta.netEmM != null)
            {
                for (PerfilClienteMapper perfilCliente : listaPerfilClienteMapper)
                {
                    if (conta.netEmM > perfilCliente.rendaMinima && conta.netEmM <= perfilCliente.rendaMaxima)
                        conta.perfilCliente = perfilCliente.recordId;
                }
            }
        }
    }

    public void atribuirMembrosConta(List<ContaMapper> listaContaMapper)
    {
        List<String> listaEquipes = this.gerarListaNomeEquipes(listaContaMapper);
        Set<String> setIdConta = this.gerarSetIdConta(listaContaMapper);

        if (listaEquipes.size() > 0)
        {
            Map<String, MembroEquipeMapper> mapaContaIdComMembroEquipe = this.gerarMapaContaIdComMembroEquipe(listaEquipes, listaContaMapper);

            List<AccountTeamMemberMapper> listaAccountTeam = this.gerarListaAccountTeamMember(mapaContaIdComMembroEquipe);
            
            this.accountTeamMemberServiceInstance.deletarAccountTeamMemberPorIdConta(setIdConta);
            this.accountTeamMemberServiceInstance.inserirAtualizarAccountTeamMemberPorChaveUsuarioConta(listaAccountTeam);
        }
    }

    public void atribuirCpfOuCnpjEmIdChaveExterna(List<ContaMapper> listaContaMapper)
    {
        for (ContaMapper conta : listaContaMapper)
        {
            if (conta.cnpj == '-' || String.isBlank(conta.cnpj))
                conta.idChaveExterna = conta.cpf;
            else 
                conta.idChaveExterna = conta.cnpj;
        }
    }

    public void tratarCodigoAssessor(List<ContaMapper> listaContaMapper) 
    {
        for (ContaMapper conta : listaContaMapper)
        {
            if(conta.codigoAssessorCrmXp == null)
                continue;
            
            Matcher matcher = Pattern.compile('[A]\\d{4,}').matcher(conta.tags);
            Boolean result = matcher.find();

            if(result)
                conta.codigoAssessor = matcher.group(0).remove('A');
            else 
                conta.codigoAssessor = conta.codigoAssessorCrmXp.remove('A');
           
        }
    }

    public void atribuirProprietarioContaPeloCodigoAssessor(List<ContaMapper> listaContaMapper) 
    {
        List<String> listaCodigoAssessor = new List<String>();

        IUserDomain usuarioContexto = this.userServiceInstance.obterUsuarioComId(UserInfo.getUserId());

        for (ContaMapper conta : listaContaMapper)
        {
            if (conta.codigoAssessor == null)
            {
                conta.codigoAssessor = String.valueOf( usuarioContexto.obterMapper().codigoAssessor);
            }else {
                listaCodigoAssessor.add(conta.codigoAssessor);
            }
        }

        // Map<String, IUserDomain> usuarios = this.userServiceInstance.obterMapaUsuariosPorCodigoAssessor(new List<String>( mapaContasComCodigoAsessor.keySet()));
        Map<String, IUserDomain> mapaCodigoAssessorUsuario = this.userServiceInstance.obterMapaUsuariosPorCodigoAssessor(listaCodigoAssessor);
        String idProprietarioAlternativo = [SELECT id, Name FROM User Where Email = 'mauro.araripe@c3csoftware.com.br' AND LastName = 'Araripe'].Id;

        for (ContaMapper conta : listaContaMapper)
        {
            if(conta.codigoAssessor != null)
            {
                String codigoAssessorDaConta = conta.codigoAssessor;
                
                IUserDomain idUsuarioDonoConta =  mapaCodigoAssessorUsuario.get(codigoAssessorDaConta + '.0');

                conta.proprietarioConta = idUsuarioDonoConta != null ? idUsuarioDonoConta.obterMapper().idRegistro : idProprietarioAlternativo;
            }

        }
    }

    private List<AccountTeamMemberMapper> gerarListaAccountTeamMember(Map<String, MembroEquipeMapper> mapaContaIdComMembroEquipe)
    {
        List<AccountTeamMemberMapper> listaAccountTeam = new List<AccountTeamMemberMapper>();
        String idConta;

        for (String key : mapaContaIdComMembroEquipe.keySet())
        {
            IAccountTeamMemberDomain accountTeamMemberDomain = (IAccountTeamMemberDomain) C3C_DIContainer_Injector.newInstance('AccountTeamMemberDomain');
            MembroEquipeMapper membroEquipeMapper = mapaContaIdComMembroEquipe.get(key);
            idConta = key.replace('-','').replace(membroEquipeMapper.recordId, '');
            listaAccountTeam.add(accountTeamMemberDomain.criarAccountTeamMemberMapperComPermissaoEdit(membroEquipeMapper.idUsuario, idConta, membroEquipeMapper.nomePapelEquipe));
        }

        return listaAccountTeam;
    }

    private Map<String, MembroEquipeMapper> gerarMapaContaIdComMembroEquipe(List<String> listaEquipes, List<ContaMapper> listaContaMapper)
    {
        Map<String, MembroEquipeMapper> mapaContaIdComMembroEquipe = new Map<String, MembroEquipeMapper>();
        List<MembroEquipeMapper> listaMembroEquipeMapper = this.membroEquipeServiceInstance.obterMembroEquipePorEquipes(listaEquipes);

        for (ContaMapper conta : listaContaMapper)
        {
            for (MembroEquipeMapper membroEquipe : listaMembroEquipeMapper)
            {
                if (conta.equipe == membroEquipe.nomeEquipe)
                    mapaContaIdComMembroEquipe.put(conta.recordId+'-'+membroEquipe.recordId, membroEquipe);
            }
        }

        return mapaContaIdComMembroEquipe;
    }

    private Set<String> gerarSetIdConta(List<ContaMapper> listaContaMapper)
    {
        Set<String> setIdConta = new Set<String>();
        for (ContaMapper conta : listaContaMapper)
        {
            if (!String.isBlank(conta.recordId))
                setIdConta.add(conta.recordId);
        }

        return setIdConta;
    }
    
    private List<String> gerarListaNomeEquipes(List<ContaMapper> listaContaMapper)
    {
        List<String> listaEquipes = new List<String>();
        for (ContaMapper conta : listaContaMapper)
        {
            if (!String.isBlank(conta.equipe))
                listaEquipes.add(conta.equipe);
        }

        return listaEquipes;
    }

