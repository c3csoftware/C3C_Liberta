public without sharing class LeadFilhoDomain implements ILeadFilhoDomain, C3C_DI_IProvider
{
    public class LeadFilhoDomainException extends Exception {}

    public LeadFilhoMapper leadFilhoMapper {get; set;}
    public List<LeadFilhoMapper> listLeadFilhoMapper {get; set;}

    //Contrutores
    public LeadFilhoDomain() 
    {}
    
    private LeadFilhoDomain(SObject leadFilhoSObject)
    {
        this.construirDomainComSObject(leadFilhoSObject);
    }

    private LeadFilhoDomain(LeadFilhoMapper leadFilhoMpp)
    {
        this.leadFilhoMapper = leadFilhoMpp;
    }

    // Métodos 

    public Object newInstance(Object params)
    {
        if(params instanceOf List<SObject>)
            return this.construirListaDomainComListaSObject((List<SObject>) params);
            
        if(params instanceOf SObject)
            return new LeadFilhoDomain((SObject) params);

        return null;
    }

    //Responsável por retornar a mapper
    public leadFilhoMapper obterMapper()
    {
        return this.leadFilhoMapper;
    }

    //Responsável por retornar o registro SObject
    public SObject obterRegistro()
    {
        return this.leadFilhoMapper.record;
    }

    //Responsável por construir a domain com sobject
    private void construirDomainComSObject(SObject leadFilhoSObject)
    {
        this.leadFilhoMapper = (LeadFilhoMapper) C3C_DIContainer_Injector.newInstance('LeadFilhoMapper', leadFilhoSObject);
    }

    //Responsável por construir uma lista de domain com uma lista de sobject
    private List<LeadFilhoDomain> construirListaDomainComListaSObject(List<SObject> listaLeadsFilhoSObject)
    {
        List<LeadFilhoDomain> listaDeLeadsFilhoDomain = new List<LeadFilhoDomain>();

        for(SObject leadFilhoSObject : listaLeadsFilhoSObject)
            listaDeLeadsFilhoDomain.add(new LeadFilhoDomain(leadFilhoSObject));

        return listaDeLeadsFilhoDomain;
    }

    public LeadFilhoMapper criarLeadFilhoComChaveLeadLista(Id idRegistro, String idListaActive, String status)
    {
        this.leadFilhoMapper = (LeadFilhoMapper) C3C_DIContainer_Injector.newInstance('LeadFilhoMapper', new Map<String, Object>{
            'idRegistro' => idRegistro,
            'idListaActive' => idListaActive, 
            'chaveLeadListaActive' => idRegistro + '-' + idListaActive,
            'status' => status
        });
        
        return this.leadFilhoMapper;
    }
    
    public LeadFilhoMapper criarLeadFilhoComChaveLeadLista(Id idRegistro, String idListaActive, String status, String proprietario)
    {
        this.leadFilhoMapper = (LeadFilhoMapper) C3C_DIContainer_Injector.newInstance('LeadFilhoMapper', new Map<String, Object>{
            'idRegistro' => idRegistro,
            'idListaActive' => idListaActive, 
            'chaveLeadListaActive' => idRegistro + '-' + idListaActive,
            'status' => status,
            'proprietario' => proprietario
        });
        
        return this.leadFilhoMapper;
    }

    public List<LeadFilhoMapper> construirListLeadFilhoMapper(List<SObject> listConta)
    {
        this.listLeadFilhoMapper = listConta != null ? (List<LeadFilhoMapper>) C3C_DIContainer_Injector.newInstance('LeadFilhoMapper', listConta) : new List<LeadFilhoMapper>();
        
        return this.listLeadFilhoMapper;
    }
    
