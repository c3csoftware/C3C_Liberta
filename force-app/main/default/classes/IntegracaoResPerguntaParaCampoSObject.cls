public without sharing class IntegracaoResPerguntaParaCampoSObject implements IIntegracaoResPerguntaParaCampoSObject
{
    //
    //Métodos
    //

    //Responsável por integrar as respostas das perguntas com os campos SObject
    public void integrarRespostasPerguntaComCamposSobject(List<IInstanciaRespostaDomain> listaNovasRespostas)
    {
        //Criando container de DML para atualização dos SObjects
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Case.SObjectType,
            Lead.SObjectType,
            Contact.SObjectType,
            Account.SObjectType,
            Opportunity.SObjectType
        });

        //Criando váriaveis lógicas
        Set<String> listaPerguntaId = new Set<String>();
        List<String> listaTarefaId = new List<String>();
        Map<String, IPerguntaDomain> mapaPerguntas = new Map<String, IPerguntaDomain>();
        Map<String, ITaskDomain> mapaTarefas = new Map<String, ITaskDomain>();
        Map<String, SObject> mapaObjetoAtualizar = new Map<String, SObject>();

        //Pegando ids perguntas, ids tarefas
        for(IInstanciaRespostaDomain novaResposta : listaNovasRespostas)
        {
            InstanciaRespostaMapper resposta = novaResposta.obterMapper();
            listaPerguntaId.add(resposta.pergunta);
            listaTarefaId.add(resposta.idTarefa);
        }

        //Construindo mapa com as perguntas
        for(IPerguntaDomain pergunta : this.instanciaPerguntaService.obterPeguntasComListaIds(listaPerguntaId))
            mapaPerguntas.put(pergunta.obterMapper().recordId, pergunta);

        for(ITaskDomain tarefa : this.instanciaBuscarInformacoesTarefaService.obterTarefaPorListId(listaTarefaId))
            mapaTarefas.put(tarefa.obterMapper().recordId, tarefa);

        //Atualizando campo SObject
        for(IInstanciaRespostaDomain novaResposta : listaNovasRespostas)
        {
            InstanciaRespostaMapper resposta = novaResposta.obterMapper();
            
            //Verificando se a pergunta e tarefa foram encontradas
            if(!mapaPerguntas.containsKey(resposta.pergunta) || !mapaTarefas.containsKey(resposta.idTarefa))
                continue;
            
            PerguntaMapper pergunta = mapaPerguntas.get(resposta.pergunta).obterMapper();
            TaskMapper tarefa = mapaTarefas.get(resposta.idTarefa).obterMapper();

            //Verificando se a pergunta atualiza campos SObject
            if(pergunta.atualizarCampoSObject)
            {
                String objetoRelativoTarefa = tarefa.relativo != null ? ((Id) tarefa.relativo).getSObjectType().getDescribe().getName() : null;
                String objetoRelacionadoTarefa = tarefa.whoId != null ? ((Id) tarefa.whoId).getSObjectType().getDescribe().getName() : null;

                if(objetoRelativoTarefa == null && objetoRelacionadoTarefa == null)
                    continue;

                String nomeObjetoAtualizar = pergunta.nomeAPIObjetoAtualizacaoCampo;
                String nomeCampoAtualizar = pergunta.nomeAPICampoAtualizacaoCampo;

                String valorResposta = resposta.respostaAlternativa != null ? resposta.respostaAlternativaValor : resposta.respostaDissertativa;

                if(valorResposta == null)
                    continue;

                System.debug(tarefa.relativo);
                System.debug(objetoRelativoTarefa);
                System.debug(tarefa.whoId);
                System.debug(objetoRelacionadoTarefa);
                System.debug(nomeObjetoAtualizar);

                SObject objetoAtualizar = null;

                if(objetoRelativoTarefa == nomeObjetoAtualizar)
                {
                    if(mapaObjetoAtualizar.containsKey(tarefa.relativo))
                        objetoAtualizar = mapaObjetoAtualizar.get(tarefa.relativo);

                    objetoAtualizar = this.criarSObjectComIdPreenchendoCampo(objetoAtualizar, objetoRelativoTarefa, tarefa.relativo, nomeCampoAtualizar, valorResposta);
                }

                if(objetoRelacionadoTarefa == nomeObjetoAtualizar)
                {
                    if(mapaObjetoAtualizar.containsKey(tarefa.whoId))
                        objetoAtualizar = mapaObjetoAtualizar.get(tarefa.whoId);

                    objetoAtualizar = this.criarSObjectComIdPreenchendoCampo(objetoAtualizar, objetoRelacionadoTarefa, tarefa.whoId, nomeCampoAtualizar, valorResposta);
                }

                if(objetoAtualizar != null)
                {
                    mapaObjetoAtualizar.put(objetoAtualizar.Id, objetoAtualizar);
                    uow.registerDirty(objetoAtualizar);
                }
            }
        }

        uow.commitWork();
        System.debug('ERA para comitar');
    }

    //Responsável por criar SObject e preencher Id e campo que será atualizado.
    private SObject criarSObjectComIdPreenchendoCampo(SObject objetoAtualizar, String nomeObj, String idObj, String nomeCampo, String valorCampo)
    {
        if(objetoAtualizar == null)
            objetoAtualizar = Schema.getGlobalDescribe().get(nomeObj).newSObject();

        objetoAtualizar.put('Id', idObj);
        objetoAtualizar.put(nomeCampo, valorCampo);

        System.debug(objetoAtualizar);

        return objetoAtualizar;
    }

