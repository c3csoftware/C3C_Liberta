    @AuraEnabled(cacheable = true)
    public static List<SObject> findRecords(String searchKey, String objectName, String fields, List<String> searchFields, Integer limitSearch, String whereSearch)
    {
        String key = '%' + searchKey + '%';

        String searchFieldsQuery = '';

        for(Integer i = 0; i < searchFields.size(); i++)
        {
            if(i > 0)
                searchFieldsQuery += ' AND ';
            searchFieldsQuery += searchFields[i] +' LIKE :key';
        }

        String query = 'SELECT Id, '+String.join(searchFields, ',')+
            (fields != null ? ','+fields : '')
        +' FROM '+objectName +' WHERE '+
        searchFieldsQuery
        +' '+whereSearch+' LIMIT :limitSearch';

        System.debug(query);
        List<SObject> sObjectList = Database.query(query);

        return sObjectList;
    }

