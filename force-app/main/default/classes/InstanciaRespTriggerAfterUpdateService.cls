public without sharing class InstanciaRespTriggerAfterUpdateService  implements IInstanciaRespTriggerAfterUpdateService 
{
    public void mudarStatusTarefaParaCompletado(List<IInstanciaRespostaDomain> listaInstanciaRespostaDomain)
    {
        Map<String, Object> mapaParaAtualizar = new Map<String, Object>();
        for (IInstanciaRespostaDomain instanciaRespostaDomain : listaInstanciaRespostaDomain)
        {
            InstanciaRespostaMapper instanciaRespostaMapp = instanciaRespostaDomain.obterMapper();
            mapaParaAtualizar.put(instanciaRespostaMapp.idTarefa, 'Completed');
            
        }

        if (mapaParaAtualizar.size() > 0)
            this.alterarStatusTarefaServiceInstance.mudarStatusTask(mapaParaAtualizar);
        
    }

    public void mudarStatusLeadParaPrimeiroContato(List<IInstanciaRespostaDomain> listaInstanciaRespostaDomain)
    {
        List<String> listaIdTask = new List<String>();
        for (IInstanciaRespostaDomain instanciaRespostaDomain : listaInstanciaRespostaDomain)
        {
            InstanciaRespostaMapper instanciaRespostaMapp = instanciaRespostaDomain.obterMapper();
            listaIdTask.add(instanciaRespostaMapp.idTarefa);
        }

        if (listaIdTask.size() > 0)
        {
            List<ITaskDomain> listaTaskDomain =  this.buscarInformacoesTarefaServiceInstance.obterTarefaPorListId(listaIdTask);
            Map<String, Object> mapaParaAtualizar = new Map<String, Object>();
            
            for (ITaskDomain taskDomain : listaTaskDomain)
            {
                TaskMapper taskMpp = taskDomain.obterMapper();
                if(taskMpp.whoId != null && taskMpp.tipoEmTipoTarefa == 'Qualificacao de Lead')
                    mapaParaAtualizar.put(taskMpp.whoId, 'Primeiro contato');
            }

            if (mapaParaAtualizar.size() > 0)
                this.alterarStatusLeadServiceInstance.mudarStatusLeadNovoParaPrimeiroContato(mapaParaAtualizar);
        }
    }

    public void mudarStatusCasoParaEncerrado(List<IInstanciaRespostaDomain> listaInstanciaRespostaDomain)
    {
        List<String> listaIdTask = new List<String>();
        for (IInstanciaRespostaDomain instanciaRespostaDomain : listaInstanciaRespostaDomain)
        {
            InstanciaRespostaMapper instanciaRespostaMapp = instanciaRespostaDomain.obterMapper();
            listaIdTask.add(instanciaRespostaMapp.idTarefa);
        }

        if (listaIdTask.size() > 0)
        {
            List<ITaskDomain> listaTaskDomain =  this.buscarInformacoesTarefaServiceInstance.obterTarefaPorListId(listaIdTask);
            Map<String, Object> mapaParaAtualizar = new Map<String, Object>();
            
            for (ITaskDomain taskDomain : listaTaskDomain)
            {
                TaskMapper taskMpp = taskDomain.obterMapper();
                if(taskMpp.relativo != null && taskMpp.tipoEmTipoTarefa == 'NPS do Assessor')
                    mapaParaAtualizar.put(taskMpp.relativo, 'Encerrado');
            }

            if (mapaParaAtualizar.size() > 0)
                this.instanciaAlterarStatusCasosService.mudarStatusCasoResolvidoParaEncerrado(mapaParaAtualizar);
        }
    }

