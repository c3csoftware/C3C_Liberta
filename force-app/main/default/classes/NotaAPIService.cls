    //
    //Métodos
    //

    public void registrarNotaDoActiveCampaign(NotaActiveCampaignRequest notaActiveCampaign)
    {
        //Instanciando UOW para trabalhar com Negócio
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Nota__c.SObjectType
        });

        //Buscando negócio na base de dados
        NegocioDomain negocio = instanciaNegocioService.obterNegocioPorIdNegocioActiveCampaign(notaActiveCampaign.dealId);

        if(negocio != null && negocio.recordId != null)
        {
            //Caso négocio exista na base de dados
            //Buscar nota na base de dados
            NotaDomain nota = this.instanciaNotaService.obterNotaComActiveCampaignId(notaActiveCampaign.id);

            if(nota != null && nota.recordId != null)
            {
                //Definido texto da nota
                nota.definirTextoNota(notaActiveCampaign.text);

                uow.registerDirty(nota.record);
            }
            else
            {
                //Caso nota não exista na base de dados   
            
                //Criando nota
                nota = (NotaDomain) C3C_DIContainer_Injector.newInstance('NotaDomain', new Map<String, Object>{
                    'notaActiveCampaingId' => notaActiveCampaign.id,
                    'negocioId' => negocio.recordId
                });

                //Definindo noma da nota
                nota.definirNomeNota(notaActiveCampaign.owner_firstname+' '+notaActiveCampaign.owner_lastname);

                //Definido texto da nota
                nota.definirTextoNota(notaActiveCampaign.text);

                //Buscar lead pelo contactid do ActiveCampaign
                LeadMapper leadRelacionadoActiveCampaign = this.instanciaLeadService.obterLeadPorActiveCampaignId(notaActiveCampaign.contactid);

                if(leadRelacionadoActiveCampaign != null && leadRelacionadoActiveCampaign.recordId != null)
                {
                    if(leadRelacionadoActiveCampaign.convertido)
                        nota.contaId = leadRelacionadoActiveCampaign.contaId;
                    else
                        nota.leadId = leadRelacionadoActiveCampaign.recordId;
                }

                uow.registerNew(nota.record);
            }

            uow.commitWork();
        }
    }

