public without sharing class LeadShareService implements ILeadShareService
{
    //Responsável por obter os compartilahmentos do Lead com ids de Lead
    public List<LeadShareDomain> obterCompartilhamentoLeadComIdsLead(Set<String> leadIds)
    {
        return LeadShareDomainFactory.construirListaCompartilhamentoAPartirListaSobject(this.instanciaLeadShareSelector.buscarCompartilhamentoLeadComIdsLead(leadIds));
    }

    //Responsável por obter um mapa de compartilhamento de lead com usuário Hunter
    public Map<String, String> obterMapaCompartilhamentoLeadUsuarioHunter(Set<String> leadIds)
    {
        //Variável responsável por armazenar mapa de leads que estão compartilhados com os usuários HUnter
        Map<String, String> mapaLeadCompartilhadoHunter = new Map<String, String>();

        //Variável responsável por mapear os usuários compartilhados do lead
        Map<String, String> mapaUsuarioCompartilhadoLead = new Map<String, String>();

        //Responsável por preencher o mapa com os usuários compartilhados do lead
        for(LeadShareDomain compartilhamentoLead : this.obterCompartilhamentoLeadComIdsLead(leadIds))
        {
            mapaUsuarioCompartilhadoLead.put(compartilhamentoLead.idUserOuGroup, compartilhamentoLead.leadId);
        }

        //Responsável por buscar usuários HUnter que estão compartilhados com lead
        for(UserMapper usuarioHunter : this.instanciaUsuarioHunterService.obterUsuariosHunterPorIds(mapaUsuarioCompartilhadoLead.keySet()))
        {
            String idLeadCompartilhadoHunter = mapaUsuarioCompartilhadoLead.get(usuarioHunter.idRegistro);
            mapaLeadCompartilhadoHunter.put(idLeadCompartilhadoHunter, usuarioHunter.idRegistro);
        }

        return mapaLeadCompartilhadoHunter;
    }

    private ILeadShareSelector instanciaLeadShareSelector
    {
        get
        {
            if(this.instanciaLeadShareSelector == null)
                this.instanciaLeadShareSelector = (ILeadShareSelector) C3C_DIContainer_Injector.getInstance('LeadShareSelector');

            return this.instanciaLeadShareSelector;
        }
        set;
    }

    private IUsuarioHunterService instanciaUsuarioHunterService
    {
        get
        {
            if(this.instanciaUsuarioHunterService == null)
                this.instanciaUsuarioHunterService = (IUsuarioHunterService) C3C_DIContainer_Injector.getInstance('UsuarioHunterService');

            return this.instanciaUsuarioHunterService;
        }
        set;
    }

