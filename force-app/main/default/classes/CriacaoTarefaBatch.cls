global without sharing class CriacaoTarefaBatch implements Database.Batchable<sObject> 
{
    global List<SObject> start(Database.BatchableContext BC)
    {
        Map<String, Schema.SObjectField> mapaCampos = Schema.getGlobalDescribe().get('Account').getDescribe().fields.getMap();
        
        String camposData = '';

        for(Schema.SObjectField campo : mapaCampos.values() )
        {
            if('DATE' == String.valueOf(campo.getDescribe().getType()))
                camposData += ', '+campo.getDescribe().getName();
        }

        return Database.query('SELECT Id, PerfilCliente__c, OwnerId'+camposData+' FROM Account WHERE Id = \'0018G00000Af3q4QAB\'');
    }
  
    global void execute(Database.BatchableContext BC, List<SObject> scope)
    {
        try
        {
            if(scope.size() > 0)
                this.criacaoTarefaServiceInstance.gerarTarefas(scope);
        }
        catch(Exception e)
        {
            System.debug('e: ' + e.getMessage()+ '; n: '+e.getStackTraceString());
            System.abortJob(bc.getJobId());
        }
    }

    global void finish(Database.BatchableContext BC){}

