    public void pegarLeadPorId(Id leadId)
    {
        ILeadDomain leadDomain = (ILeadDomain) C3C_DIContainer_Injector.newInstance('LeadDomain');
        this.leadParaConverter = (LeadMapper) leadDomain.construirLeadMapper(this.leadSelectorInstance.obterLeadPorIdsDoLead(new List<String>{(String) leadId})[0]);
    }

    public PageReference converterLead()
    {        
        if (this.leadParaConverter.verificarInvalidadeCpf())
        {
            throw new ConverterLeadServiceException('Lead não pode ser convertido pois o CPF está inválido. Informe um cpf válido ou entre em contato com o Adminstrador do Sistema');
        }
        
        if(!this.instanciaInstanciaRespostaService.verificarPopAssessorRespondido(this.leadParaConverter.recordId))
        {
            throw new ConverterLeadServiceException('Lead não pode ser convertido pois no POP foi selecionado Não Convertido!');
        }

        else 
        {
            Database.LeadConvert lc = new Database.LeadConvert();
            String masterLabelLeadStatus = leadStatusServiceInstance.retornarLabelDeLeadConvertido();
            lc.setLeadId(this.leadParaConverter.recordId);
            lc.setDoNotCreateOpportunity(true);    
            lc.setConvertedStatus(masterLabelLeadStatus);

            Database.LeadConvertResult lcr = Database.convertLead(lc);
            System.assert(lcr.isSuccess());
    
            String accountId = lcr.getAccountId();
            this.accountServiceInstance.porNomeEChaveEmConta(accountId, this.leadParaConverter);
            atribuirProprietarioESetarCodigoAssessor(accountId, this.leadParaConverter);
            
            PageReference pageRef = new PageReference('/' + accountId);    
            pageRef.setRedirect(true);

            return pageRef; 
        }
    }

    public String converterLeadComMerge(String idRegistro)
    {
        try 
        {
            List<ILeadDomain> listLeadDomain = this.instanciaBuscarInformacoesLeadService.obterLeadPorIdsDoLead(new List<String>{idRegistro});
            String idLead;
            String documento;

            for(ILeadDomain itemLeadDomain : listLeadDomain)
            {
                LeadMapper leadMapper = itemLeadDomain.obterMapper();
                idLead = leadMapper.recordId;
                documento = String.isBlank(leadMapper.cpf) ? leadMapper.cnpj : leadMapper.cpf;
            }

            IContaDomain contaDomain = this.instanciaBuscarInformacoesContaService.obterContaPorIdChaveExterna(documento);

            Database.LeadConvert lc = new database.LeadConvert();
            lc.setLeadId(idRegistro);
            lc.setConvertedStatus(leadStatusServiceInstance.retornarLabelDeLeadConvertido());
            lc.setAccountId(contaDomain.obterMapper().recordId);


            Database.LeadConvertResult lcr = Database.convertLead(lc);
            System.assert(lcr.isSuccess());

            C3C_APP_ApplicationContext.setResponse(contaDomain.obterMapper().recordId);
            return contaDomain.obterMapper().recordId;
        } 
        catch (Exception e) 
        {
            C3C_APP_ApplicationContext.addCriticalError('400', 'Ocorreu uma exceção ao tentar converter o Lead, err: '+e.getMessage());
            return null;
        }
        
    }

    private void atribuirProprietarioESetarCodigoAssessor(String idDaConta, LeadMapper leadMapper)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Account.SObjectType
        });
        

        IAccountDomain accountDomain = (IAccountDomain) C3C_DIContainer_Injector.newInstance('AccountDomain', new Map<String, Object>{
            'idRegistro' => idDaConta,
            'leadConvertido' => leadMapper
        });

        // Atribuindo proprietário da conta setando o usuário do contexto.
        accountDomain.obterMapper().proprietarioConta = UserInfo.getUserId();
        
        // Realizar um selector para pegar o usuário e despois pegar o código que está em sua conta e setar no campo abaixo.
        IUserDomain userDomain = (IUserDomain) C3C_DIContainer_Injector.newInstance('UserDomain', this.instanciaUserSelector.obterUsuarioComId(UserInfo.getUserId()));
         
        String codigoAssessor = String.valueOf(userDomain.obterMapper().codigoAssessor.intValue());
            
        accountDomain.obterMapper().codigoAssessor = codigoAssessor;

        uow.registerDirty(accountDomain.obterRegistro());
        uow.commitWork();
    }

    public void alterarProprietarioDoLeadParaAssessor(String idUsuarioAssessor, String idLead)
    {
        if(!(idUsuarioAssessor == null || idLead == null))
        {
                // setar usuário como novo proprietário do lead
            fflib_ISObjectUnitOfWork uowLead = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
                Lead.SObjectType
            });
            fflib_ISObjectUnitOfWork uowLeadShare = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
                LeadShare.SObjectType
            });
            ILeadDomain instanciaLeadDomain = (ILeadDomain) C3C_DIContainer_Injector.newInstance('LeadDomain', this.leadSelectorInstance.obterLeadPorIdDoLead(idLead));
            if(instanciaLeadDomain.obterMapper() != null)
            {
                String idProprietarioAtualLead = instanciaLeadDomain.obterMapper().proprietario;
                System.debug('idProprietarioAtualLead:'+idProprietarioAtualLead);
                System.debug('idUsuarioAssessor:'+idUsuarioAssessor);
                instanciaLeadDomain.obterMapper().proprietario = idUsuarioAssessor;
                
                // dar permissões de lead share para proprietário anterior
                SObject compartilhamentoRegistro = Schema.getGlobalDescribe().get('LeadShare').newSObject();
        
                compartilhamentoRegistro.put('LeadId', idLead);
                compartilhamentoRegistro.put('UserOrGroupId', idProprietarioAtualLead);
                compartilhamentoRegistro.put('LeadAccessLevel', 'Edit');
        
                //adicionar registros
                if(!Test.isRunningTest())
                {
                    uowLeadShare.registerNew(compartilhamentoRegistro);
                    uowLead.registerDirty(instanciaLeadDomain.obterRegistro());
                    uowLead.commitWork();
                    uowLeadShare.commitWork();                
                }
            }
        }
       
    }
        
