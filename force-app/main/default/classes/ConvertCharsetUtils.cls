    public ConvertCharsetUtils() {}

    public static String converterIsoParaUTF8(String textoIso)
    {
        return EncodingUtil.urlDecode(EncodingUtil.urlEncode(textoIso, 'ISO_8859_1'), 'UTF8');
    }

    public static Map<String,String> converterMapaIsoParaUTF8(Map<String,String> mapaParaConverter)
    {
        Map<String,String> mapaConvertido = new Map<String,String>();
        for(String key : mapaParaConverter.keySet())
        {
            mapaConvertido.put(key, EncodingUtil.urlDecode(EncodingUtil.urlEncode(mapaParaConverter.get(key), 'ISO_8859_1'), 'UTF8'));
        }
        
        return mapaConvertido;
    }
    
    
