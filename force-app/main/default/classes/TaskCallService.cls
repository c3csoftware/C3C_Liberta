{
    //
    //Métodos
    //

    //Responsável por criar/atualizar tarefas de chamadas.
    public String criarAtualizarTarefaChamada(CallboxChamadaViewModel callboxChamadaInfo)
    {
        ILeadDomain lead = this.obterLeadComNumeroCelular(callboxChamadaInfo.numeroDestino);
        IContactDomain contato = this.obterContactComNumeroCelular(callboxChamadaInfo.numeroDestino);

        if(contato.obterMapper().idRegistro == null && lead.obterMapper() == null && callboxChamadaInfo.numeroDestino.length() >= 8 && callboxChamadaInfo.registroRelacionado == null)
        {
            contato = this.instanciaContactService.criarNovoContato(ContactDomainFactory.novaInstanciaContatoComNumeroCelular(
                callboxChamadaInfo.numeroDestino
            ));
        }

        String idNomeRelacionado = callboxChamadaInfo.registroRelacionado != null ? callboxChamadaInfo.registroRelacionado : callboxChamadaInfo.numeroDestino.length() < 8 ? null : (contato.obterMapper().idRegistro != null ? contato.obterMapper().idRegistro : lead.obterMapper().recordId);

        ITaskCallDomain tarefaChamadaDomain = TaskCallFactory.novaInstanciaComCallboxChamadaViewModel(
            callboxChamadaInfo,
            'Ligação',
            'Normal',
            callboxChamadaInfo.direcaoChamada == 'in' ? 'Inbound' : 'Outbound',
            'Call',
            'Completed',
            idNomeRelacionado,
            System.today()
        );

        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Task.SObjectType
        });

        uow.registerUpsert(tarefaChamadaDomain.obterRegistro());

        uow.commitWork();

        String idTarefaChamada = tarefaChamadaDomain.obterRegistro().Id;

        C3C_APP_ApplicationContext.setResponse(idTarefaChamada);

        return idTarefaChamada;
    }

    //Responsável transferir tarefas de chamadas.
    public void tranferirChamada(String idTarefaChamada, String ramalTransferencia)
    {
        ITaskCallDomain tarefaChamadaDomain = TaskCallFactory.novaInstanciaComIdTarefa(idTarefaChamada, ramalTransferencia);

        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Task.SObjectType
        });

        if(tarefaChamadaDomain.obterRegistro().Id == null)
            uow.registerNew(tarefaChamadaDomain.obterRegistro());
        else
            uow.registerDirty(tarefaChamadaDomain.obterRegistro());

        uow.commitWork();
    }

    //Responsável por criar tarefa de ligação redirecionando
    public void criarAtualizarTarefaRedirecionamentoLigacao(String idTarefa, String assunto, String numeroDestino, Boolean redirecionamentoLigacao)
    {
        ILeadDomain lead = this.obterLeadComNumeroCelular(numeroDestino);
        IContactDomain contato = this.obterContactComNumeroCelular(numeroDestino);

        if(contato.obterMapper().idRegistro == null && lead.obterMapper() == null && numeroDestino.length() >= 8)
        {
            contato = this.instanciaContactService.criarNovoContato(ContactDomainFactory.novaInstanciaContatoComNumeroCelular(
                numeroDestino
            ));
        }

        String idNomeRelacionado = contato.obterMapper().idRegistro != null ? contato.obterMapper().idRegistro : lead.obterMapper().recordId;

        UserMapper usuario = this.instanciaUserService.obterUsuarioComId(UserInfo.getUserId()).obterMapper();

        ITaskCallDomain tarefaChamadaDomain = TaskCallFactory.novaInstanciaTarefaRedirecionamento(idTarefa, assunto, idNomeRelacionado, numeroDestino, usuario.ramal, redirecionamentoLigacao);

        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            Task.SObjectType
        });

        uow.registerUpsert(tarefaChamadaDomain.obterRegistro());

        uow.commitWork();
    }

    private ILeadDomain obterLeadComNumeroCelular(String numeroCelular)
    {
        return (ILeadDomain) C3C_DIContainer_Injector.newInstance('LeadDomain', this.instanciaLeadSelector.buscarLeadComNumeroCelular(numeroCelular));
    }

    private IContactDomain obterContactComNumeroCelular(String numeroCelular)
    {
        return (IContactDomain) C3C_DIContainer_Injector.newInstance('ContactDomain', this.instanciaContactSelector.buscarContatoComNumeroCelular(numeroCelular));
    }

    //Responsável por retornar as tarefas do tipo "Chamada" do usuário atual
    public List<TarefaChamadaViewModel> obterTarefasChamadasDoUsuarioAtual()
    {
        List<TarefaChamadaViewModel> listaTarefasChamada = new List<TarefaChamadaViewModel>();

        for(SObject tarefaObj : this.instanciaTaskCallSelector.buscarTarefasChamadasComIdProprietario(UserInfo.getUserId()))
        {
            ITaskCallDomain tarefa = (ITaskCallDomain) C3C_DIContainer_Injector.newInstance('TaskCallDomain', tarefaObj);
            TaskCallMapper tarefaMapper = tarefa.obterMapper();

            listaTarefasChamada.add((TarefaChamadaViewModel) C3C_DIContainer_Injector.newInstance('TarefaChamadaViewModel', new Map<String, Object>{
                'idTarefa' => tarefaMapper.idRegistro,
                'numeroCelular' => tarefaMapper.referenciaChamada,
                'dataLigacao' => tarefaMapper.dataInicioChamada == null ? tarefaMapper.dataCriacao.addHours(3) : tarefaMapper.dataInicioChamada.addHours(3)
            }));
        }

        System.debug(listaTarefasChamada);
        C3C_APP_ApplicationContext.setResponse(listaTarefasChamada);

        return listaTarefasChamada;
    }

