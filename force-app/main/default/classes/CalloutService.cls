public without sharing class CalloutService implements C3C_DI_IProvider
{
    public without sharing class  CalloutServiceException extends Exception{}

    public CalloutContract contrato {get; set;}

    public String requestLabel {get; set;}
    public String service {get; set;}

    public CalloutService(){}

    public CalloutService(CalloutContract contrato)
    {
        this.contrato = contrato;
    }

    public Object newInstance(Object params)
    {
        if(params instanceOf CalloutContract)
            return new CalloutService((CalloutContract) params);

        return null;
    }
    
    public HttpResponse doCallout()
    {
        //*Faz a request assyncrona 
        if(contrato == null) new CalloutServiceException('Contrato não pode ser nulo');
        Http httpProtocol = new Http();      
        HttpRequest request = new HttpRequest();
        HttpResponse response;
        
        
            System.debug('CalloutService setTimeout: ' + contrato.getTimeout());
            request.setEndPoint(contrato.getEndPoint());
            request.setTimeout(contrato.getTimeout());
            request.setMethod(contrato.getMethod());
        try
        { 
			System.debug('CalloutService request: ' + request);
            string body = contrato.getBody();
            System.debug('CalloutService body: ' + body);
            if(!string.isEmpty(body) && body != 'null')request.setBody(body);
            
            contrato.setHeader(request);
            response = httpProtocol.send(request);
            System.debug('CalloutService response: ' + response);
            return response;    
             
        }
        catch(Exception e)
        {
            throw new CalloutServiceException(e.getMessage() + ' Line: '+ e.getLineNumber()); 
        }
           
    }

    public Object processResponse(List<String> labels, Object state) {
        //*Atualiza o objeto do evento no callback e chama o método de callback do restService
        System.debug('Callback called');
        HttpResponse response = Continuation.getResponse(this.requestLabel);
        System.debug('Got response: ' + response.getBody());

        return null;
    }
    
