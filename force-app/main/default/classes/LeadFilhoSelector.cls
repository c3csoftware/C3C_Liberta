public without sharing class LeadFilhoSelector extends fflib_SObjectSelector implements ILeadFilhoSelector
{
    public class LeadFilhoSelectorException extends Exception{}
    
    public Schema.SObjectType getSObjectType()
    {
        return LeadFilho__c.sObjectType;    
    }

    public List<Schema.SObjectField> getSObjectFieldList()
    {
        return new List<Schema.SObjectField>
        {
            LeadFilho__c.Id,
			LeadFilho__c.Lead__c,
			LeadFilho__c.Status__c,
			LeadFilho__c.ChaveLeadListaActive__c,
			LeadFilho__c.NomeCampanhaFila__c,
            LeadFilho__c.OwnerId,
            LeadFilho__c.Oportunidade__c,
            LeadFilho__c.NomeLista__c,
            LeadFilho__c.ActiveCampaingId__c
        };
    }

    public SObject obterLeadFilhoPorChaveLeadLista(String chaveLeadListaActive)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'ChaveLeadListaActive__c', '=', chaveLeadListaActive);
        
        for(SObject leadFilho : (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()))
        	return leadFilho;
        
        return null; 
    }

    public SObject obterLeadFilhoPorId(String idRegistro)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'Id', '=', idRegistro);
        
        for(SObject leadFilho : (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()))
        	return leadFilho;
        
        return null; 
    }
    
    public List<SObject> obterLeadFilhoPorListaIdLead(List<String> listaIdLead)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'Lead__c', 'IN', listaIdLead);
        
        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }
    
    public List<SObject> obterLeadFilhoPorListaChaveExterna(List<String> listaChaveLeadLista)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'ChaveLeadListaActive__c', 'IN', listaChaveLeadLista);
        
        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }

    public List<SObject> buscarLeadFilhoPorListaIdLeadIdOportunidade(List<String> listaIdLeadIdOportunidade)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'Lead__c', 'IN', listaIdLeadIdOportunidade);
        query.addCondition('OR', 'Oportunidade__c', 'IN', listaIdLeadIdOportunidade);
        
        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }
    
    public List<SObject> obterLeadFilhoPorListaIdLeadFilho(List<String> listaIdLeadFilho)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'Id', 'IN', listaIdLeadFilho);

        query.selectField('Lead__r.OwnerId');
        query.selectField('Oportunidade__r.OwnerId');
        
        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }
    
    public List<SObject> obterLeadFilhoPorIdsDaListaEIdContatoActive(Set<String> listaIdDaLista, String idContatoActive)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.selectField('ListaActiveCampaign__r.IdLista__c');
        query.addCondition('', 'ListaActiveCampaign__r.IdLista__c', 'IN', listaIdDaLista);
        query.addCondition('', 'Lead__r.ActiveCamp__Active_Campaign_Id__c', '=', idContatoActive);
        query.addCondition('', 'Oportunidade__r.IdContatoActive__c', '=', idContatoActive);
        query.setLogic('{0} AND ({1} OR {2})');
        
        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }
    
