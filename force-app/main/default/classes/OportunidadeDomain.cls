public without sharing class OportunidadeDomain implements IOportunidadeDomain, C3C_DI_IProvider
{
    public class OportunidadeDomainException extends Exception {}

    public OportunidadeMapper oportunidadeMapper {get; set;}
    public List<OportunidadeMapper> listOportunidadeMapper {get; set;}

    //Contrutores
    public OportunidadeDomain() 
    {}
    
    private OportunidadeDomain(SObject oportunidadeSObject)
    {
        this.construirDomainComSObject(oportunidadeSObject);
    }
    
    private OportunidadeDomain(String nome, String chaveExterna, String contaId, String fase, Date dataFechamento, String idContatoActive, String nomeLista)
    {
        this.construirDomainComNome(nome, chaveExterna, contaId, fase, dataFechamento, idContatoActive, nomeLista);
    }

    private OportunidadeDomain(OportunidadeMapper oportunidadeMpp)
    {
        this.oportunidadeMapper = oportunidadeMpp;
    }
    
    private OportunidadeDomain(Id idRegistro, Id idProprietario)
    {
        this.construirDomainComIdEProprietario(idRegistro, idProprietario);
    }

    // Métodos 
    public Object newInstance(Object params)
    {
        if(params instanceOf List<SObject>)
            return this.construirListaDomainComListaSObject((List<SObject>) params);
            
        if(params instanceOf SObject)
            return new OportunidadeDomain((SObject) params);

        if(params instanceOf Map<String,Object>)
        {
            Map<String,Object> mapaParams = (Map<String,Object>) params;
            if(mapaParams.containsKey('idRegistro'))
                return new OportunidadeDomain((Id) mapaParams.get('idRegistro'), (Id) mapaParams.get('idProprietario'));
            return new OportunidadeDomain((String) mapaParams.get('nome'), (String) mapaParams.get('chaveExterna'), (String) mapaParams.get('contaId'), (String) mapaParams.get('fase'), (Date) mapaParams.get('dataFechamento'), (String) mapaParams.get('idContatoActive'), (String) mapaParams.get('nomeLista'));
        }

        return null;
    }

    //Responsável por preencher o id do contato
    public void preencherNovoId()
    {
        this.oportunidadeMapper.recordId = this.oportunidadeMapper.record.Id;
    }

    //Responsável por retornar a mapper
    public OportunidadeMapper obterMapper()
    {
        return this.oportunidadeMapper;
    }

    //Responsável por retornar o registro SObject
    public SObject obterRegistro()
    {
        return this.oportunidadeMapper.record;
    }

    //Responsável por construir a domain com sobject
    private void construirDomainComSObject(SObject oportunidadeSObject)
    {
        this.oportunidadeMapper = (OportunidadeMapper) C3C_DIContainer_Injector.newInstance('OportunidadeMapper', oportunidadeSObject);
    }
        
    //Responsável por construir a domain com idRegistro e idProprietario
    private void construirDomainComIdEProprietario(Id idRegistro, Id idProprietario)
    {
        this.oportunidadeMapper = (OportunidadeMapper) C3C_DIContainer_Injector.newInstance('OportunidadeMapper', new Map<String,Object>{
            'idRegistro' => idRegistro,
            'idProprietario' => idProprietario
        });
    }

    //Responsável por construir uma lista de domain com uma lista de sobject
    private List<OportunidadeDomain> construirListaDomainComListaSObject(List<SObject> listaOportunidadesSObject)
    {
        List<OportunidadeDomain> listaDeOportunidadesDomain = new List<OportunidadeDomain>();

        for(SObject oportunidadeSObject : listaOportunidadesSObject)
            listaDeOportunidadesDomain.add(new OportunidadeDomain(oportunidadeSObject));

        return listaDeOportunidadesDomain;
    }

    //Responsável por construir a domain com nome
    private void construirDomainComNome(String nome, String chaveExterna, String contaId, String fase, Date dataFechamento, String idContatoActive, String nomeLista)
    {
        this.oportunidadeMapper = (OportunidadeMapper) C3C_DIContainer_Injector.newInstance('OportunidadeMapper', new Map<String,Object>{
            'nome' => nome,
            'chaveExterna' => chaveExterna,
            'contaId' => contaId,
            'fase' => fase,
            'dataFechamento' => dataFechamento,
            'idContatoActive' => idContatoActive,
            'nomeLista' => nomeLista
        });
    }

    public OportunidadeMapper criarOportunidade(String lead, String activeCampaignId, String etapaOportunidade, String nome, String funil, String fase, Decimal valor, Date dataFechamento)
    {
        this.oportunidadeMapper = (OportunidadeMapper) C3C_DIContainer_Injector.newInstance('OportunidadeMapper', new Map<String, Object>{
            'lead' => lead,
            'activeCampaignId' => activeCampaignId,
            'etapaOportunidade' => etapaOportunidade,
            'nome' => nome,
            'funil' => funil,
            'fase' => fase,
            'valor' => valor,
            'dataFechamento' => dataFechamento
        });

        return this.oportunidadeMapper;
    }
    
