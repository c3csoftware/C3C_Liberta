@RestResource(urlMapping='/oportunidade/registrar')
global class AdicionaOportunidadeService 
{
	@HttpPost
    global static String doPost()
	{
        RestRequest req = RestContext.request;
        Map<String,String> reqConvertido = ConvertCharsetUtils.converterMapaIsoParaUTF8(req.params);

        String idLead;
        String idLeadActive = (String) reqConvertido.get('contact[id]');
        
        if(idLeadActive != '')
        {
            idLead = ((LeadMapper) leadServiceInstance.obterLeadPorActiveCampaignId(idLeadActive)).recordId;
        }
        List<String> pickListValuesList = gerarListaFaseOportunidade();

        oportunidadeServiceInstance.inserirAtualizarOportunidadePorActiveCampaignId(idLead, (String) reqConvertido.get('deal[id]'), (String) reqConvertido.get('deal[stage_title]'), (String) reqConvertido.get('deal[title]'), (String) reqConvertido.get('deal[pipeline_title]'), (String) pickListValuesList[Integer.valueOf((String) reqConvertido.get('deal[status]'))], Decimal.valueOf(((String) req.params.get('deal[value]').replace('.','').replace(',', '.'))), Date.valueOf((String) reqConvertido.get('deal[create_date]')));
        
        return 'Inserido no objeto Opportunity';
    }

    private static List<String> gerarListaFaseOportunidade()
    {
        List<String> pickListValuesList = new List<String>();
		Schema.DescribeFieldResult fieldResult = Opportunity.StageName.getDescribe();
		List<Schema.PicklistEntry> ple = fieldResult.getPicklistValues();
		for( Schema.PicklistEntry pickListVal : ple)
        {
			pickListValuesList.add(pickListVal.getLabel());
		} 
        return pickListValuesList;
    }

    //Servi√ßos
    public static ILeadService leadServiceInstance 
    {
        get
        {
            if(leadServiceInstance == null)
                leadServiceInstance = (ILeadService) C3C_DIContainer_Injector.getInstance('LeadService');
            
            return leadServiceInstance;
        }
        set;
    }

    public static IOportunidadeService oportunidadeServiceInstance 
    {
        get
        {
            if(oportunidadeServiceInstance == null)
                oportunidadeServiceInstance = (IOportunidadeService) C3C_DIContainer_Injector.getInstance('OportunidadeService');
            
            return oportunidadeServiceInstance;
        }
        set;
    }
    
