public without sharing class OportunidadeService implements IOportunidadeService
{
    public void inserirAtualizarOportunidadePorActiveCampaignId(String lead, String activeCampaignId, String etapaOportunidade, String nome, String funil, String fase, Decimal valor, Date dataFechamento)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{Opportunity.SObjectType});

        IOportunidadeDomain oportunidadeDomain = (IOportunidadeDomain) C3C_DIContainer_Injector.newInstance('OportunidadeDomain');
        
        uow.registerUpsertUsingExternalId((oportunidadeDomain.criarOportunidade(lead, activeCampaignId, etapaOportunidade, nome, funil, fase, valor, dataFechamento)).record, 'ActiveCampaignId__c');

        uow.commitWork();
    }

    //Responsável por criar oportunidade relacionada ao lead
    public OportunidadeMapper criarOportunidadeRelacionadoLead(String leadId, String nome)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{Opportunity.SObjectType});

        IOportunidadeDomain oportunidadeDomain = (IOportunidadeDomain) C3C_DIContainer_Injector.newInstance('OportunidadeDomain');
        
        uow.registerNew((oportunidadeDomain.criarOportunidade(leadId, null, null, nome, null, 'Em negociação', null, System.today())).record);

        uow.commitWork();

        oportunidadeDomain.obterMapper().recordId = oportunidadeDomain.obterMapper().record.Id;

        return oportunidadeDomain.obterMapper();
    }

    //Responsável por criar oportunidade relacionada à conta
    public OportunidadeMapper criarOportunidadeRelacionadoConta(String contaId, String nome)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{Opportunity.SObjectType});

        IOportunidadeDomain oportunidadeDomain = (IOportunidadeDomain) C3C_DIContainer_Injector.newInstance('OportunidadeDomain', new Map<String, Object>{
            'nome' => nome,
            'chaveExterna' => null, 
            'contaId' => contaId,
            'fase' => null,
            'dataFechamento' => null,
            'idContatoActive' => null,
            'nomeLista' => null
        });
        
        uow.registerNew(oportunidadeDomain.obterMapper().record);

        uow.commitWork();
        
        oportunidadeDomain.obterMapper().recordId = oportunidadeDomain.obterMapper().record.Id;

        return oportunidadeDomain.obterMapper();
    }

