{
    public class NegocioServiceException extends Exception {}

    //
    //Métodos
    //

    //Responsável por obter o negócio pelo id ActiveCampaign do negócio 
    public NegocioDomain obterNegocioPorIdNegocioActiveCampaign(String idNegocioActiveCampaign)
    {
        return (NegocioDomain) C3C_DIContainer_Injector.newInstance('NegocioDomain', this.instanciaNegocioSelector.buscarNegocioPorIdNegocioActiveCampaign(idNegocioActiveCampaign));
    }

    //Resposável por obter uma lista de negócio por ids
    public List<NegocioDomain> obterListaNegocioPorListaIds(List<String> listaIdsNegocio)
    {
        return NegocioDomainFactory.construirListaNegocioDomainAPartirListaSobject(this.instanciaNegocioSelector.buscarListaDeNegociosComListaDeIds(listaIdsNegocio));
    }

    // Responsável por atribuir Negócio ao usuário que aceitar tal negócio.
    public void atribuirNegociosAoUsuario(List<String> idsNegocio, String idUsuario)
    {
        fflib_ISObjectUnitOfWork uow = (fflib_ISObjectUnitOfWork) C3C_DIContainer_Injector.newInstance('fflib_SObjectUnitOfWork', new Schema.SObjectType[]{
            LeadFilho__c.SObjectType, Lead.SObjectType, Task.SObjectType, Opportunity.SObjectType 
        });

        //Variável responsável para armanezar lista de negócios a partir dos ids de negócios informados
        List<NegocioDomain> listaNegocio = this.obterListaNegocioPorListaIds(idsNegocio);

        //Variável reponsável por armazenar um mapa de leads relacionados aos negócios
        Map<String, LeadMapper> mapaLeadRelacionadoAoNegocio = new Map<String, LeadMapper>();

        //Variável reponsável por armazenar um mapa de oportunidades relacionados aos negócios
        Map<String, OportunidadeMapper> mapaOportunidadeRelacionadoAoNegocio = new Map<String, OportunidadeMapper>();

        //Responsável por pegar os ids de leads e oportunidades relacionados ao negócio
        for(NegocioDomain negocio : listaNegocio)
        {
            if(negocio.lead != null)
                mapaLeadRelacionadoAoNegocio.put(negocio.lead, null);
            
            if(negocio.oportunidade != null)
                mapaOportunidadeRelacionadoAoNegocio.put(negocio.oportunidade, null);
            
        }

        //Responsável por buscar os leads relacionados ao negócio e preenche-los no mapa
        if (!mapaLeadRelacionadoAoNegocio.isEmpty())
        {
            for(ILeadDomain lead : this.instanciaBuscarInformacoesLeadService.obterLeadPorIdsDoLead(new List<String>(mapaLeadRelacionadoAoNegocio.keySet())))
            {
                mapaLeadRelacionadoAoNegocio.put(lead.obterMapper().recordId, lead.obterMapper());
            }   

        }
        
        //Responsável por buscar as oportunidades relacionados ao negócio e preenche-los no mapa
        if (!mapaOportunidadeRelacionadoAoNegocio.isEmpty())
        {
            for(IOportunidadeDomain oportunidade : this.instanciaBuscarInformacoesOportunidadeService.obterOportunidadePorIdsDeOportunidade(new List<String>(mapaOportunidadeRelacionadoAoNegocio.keySet())))
            {
                mapaOportunidadeRelacionadoAoNegocio.put(oportunidade.obterMapper().recordId, oportunidade.obterMapper());
            }

        }

        //Variável responsável por armazenar a fila lead sem dono
        GroupMapper filaLeadSemDono = this.instanciaGroupService.obterGroupPorNome('Lead sem Dono');
        
        //Variável responsável por armazenar mapa de leads que estão compartilhados com os usuários Hunter
        Map<String, String> mapaLeadCompartilhadoHunter = new Map<String, String>();
        if (!mapaLeadRelacionadoAoNegocio.isEmpty())
        {
            mapaLeadCompartilhadoHunter = this.instanciaLeadShareService.obterMapaCompartilhamentoLeadUsuarioHunter(mapaLeadRelacionadoAoNegocio.keySet());
        }
       
        //Variável responsável por armazenar os leads que receberão tarefa de qualificação
        List<LeadMapper> listaLeadReceberTarefaQualificacao = new List<LeadMapper>();
 
        for(NegocioDomain negocio : listaNegocio)
        {
            //Variável responsável por armazenar o lead relacionado ao negócio
            LeadMapper leadRelacionado = (LeadMapper) C3C_DIContainer_Injector.newInstance('LeadMapper');
            if (!mapaLeadRelacionadoAoNegocio.isEmpty())
            {
                leadRelacionado = mapaLeadRelacionadoAoNegocio.containsKey(negocio.lead) ? mapaLeadRelacionadoAoNegocio.get(negocio.lead): null;
            }

            //Variável responsável por armazenar a oportunidade relacionado ao negócio
            OportunidadeMapper oportunidadeRelacionado = (OportunidadeMapper) C3C_DIContainer_Injector.newInstance('OportunidadeMapper');
            if (!mapaOportunidadeRelacionadoAoNegocio.isEmpty())
            {
                oportunidadeRelacionado = mapaOportunidadeRelacionadoAoNegocio.containsKey(negocio.oportunidade) ? mapaOportunidadeRelacionadoAoNegocio.get(negocio.oportunidade): null;
            }

            // Verrifica se o nome da fila está diferente de Null, pois caso seja feito o .contains em null, ocorre erro de Referência a Null
            if (negocio.nomeFila != null)
            {
                try
                {
                    // Verificar se o negócio é do tipo Hunter e se o lookup Lead está diferente de Null
                    if(negocio.nomeFila.contains('Hunter') && negocio.lead != null)
                    {
                        if(leadRelacionado.proprietario == filaLeadSemDono.recordId && !mapaLeadCompartilhadoHunter.containsKey(negocio.lead))
                        {
                            negocio.proprietario = idUsuario;
                            leadRelacionado.proprietario = negocio.proprietario;
                            listaLeadReceberTarefaQualificacao.add(leadRelacionado);              
                        }
                        //Exception para caso haja um usuário de papel hunter
                        else if(mapaLeadCompartilhadoHunter.containsKey(negocio.lead))
                        {
                            C3C_APP_ApplicationContext.addCriticalError('400', 'O Negócio não pôde ser aceito pois já foi aceito por outro Hunter');
                        }
                        else if(((Id) leadRelacionado.proprietario).getSObjectType().getDescribe().getName() != 'Group')
                        {
                            if (leadRelacionado.proprietario == idUsuario)
                                C3C_APP_ApplicationContext.addCriticalError('400', 'Negócio já foi aceito por você');
                            else
                                C3C_APP_ApplicationContext.addCriticalError('400', 'Negócio já foi aceito por outro Hunter');
                        }
                    }
    
                    // Verifica se o negócio é um Negócio. 
                    else if (negocio.nomeFila.contains('LS') && negocio.oportunidade != null)
                    {
                        negocio.proprietario = idUsuario;
                        oportunidadeRelacionado.proprietario = idUsuario;
                    }

                    else
                    {
                        /**
                         *  TODO: 
                         * É necessário verificar essa questão, para ver se o Webhook está pegando o nome da fila de acordo
                         * com o que está em active.
                         * 
                         */
                        C3C_APP_ApplicationContext.addCriticalError('400', 'Fila não reconhecida, entre em contato com o administrador do sistema.');
                    }
    
                }
                catch(Exception e)
                {
                   C3C_APP_ApplicationContext.addCriticalError('400', e.getMessage() + ' Stack Trace: ' + e.getStackTraceString());
                } 
                uow.registerDirty(negocio.record);

            }
            else 
            {
                System.debug(LoggingLevel.ERROR, 'Erro relacionado ao nome da fila, verifique se a fila está setada corretamente');
                C3C_APP_ApplicationContext.addCriticalError('400', 'Negócio não possui Fila atribuída, entre em contato com o Administrador do Sistema');
            }
            
            if (negocio.oportunidade != null)
                uow.registerDirty(oportunidadeRelacionado.record);
            
            if (negocio.lead != null) {
                uow.registerDirty(leadRelacionado.record);
            }           
        }

        //Variável resposável por armazenar o tipo tarefa qualificação lead
        ITipoTarefaDomain tipoTarefaDomainLead = ((ITipoTarefaService) C3C_DIContainer_Injector.getInstance('TipoTarefaService')).obterTipoTarefasPorTipoIgualQualificacaoLead();

        //Resposável por criar tarefas de qualificação relacionado ao lead
        for (LeadMapper leadTarefa : listaLeadReceberTarefaQualificacao)
        {
            ITaskDomain tarefa = (ITaskDomain) C3C_DIContainer_Injector.newInstance('TaskDomain');
            TaskMapper tarefaMapper = tarefa.obterMapper();           
            
            tarefaMapper.proprietario = idUsuario;
            tarefaMapper.assunto = 'Qualificação do Lead';
            tarefaMapper.tipoTarefa = tipoTarefaDomainLead.obterMapper().recordId;
            tarefaMapper.whoId = leadTarefa.recordId;

            uow.registerNew(tarefaMapper.record);
        }
        
        try 
        {
            uow.commitWork();
        } 
        catch (Exception e) 
        {
            System.debug('400 - Ocorreu uma exceção ao tentar salvar o Negócio, err: '+e.getMessage());
        }
    }

