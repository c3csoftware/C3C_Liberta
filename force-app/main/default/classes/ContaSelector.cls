public without sharing class ContaSelector extends fflib_SObjectSelector implements IContaSelector
{
    public class ContaSelectorException extends Exception{}
    
    public Schema.SObjectType getSObjectType()
    {
        return Account.sObjectType;    
    }

    public List<Schema.SObjectField> getSObjectFieldList()
    {
        return new List<Schema.SObjectField>
        {
            Account.Id,
			Account.Codigo__c,
            Account.IdChaveExterna__c,
            Account.Equipe__c,
            Account.ClassificacaoEstrelas__c,
            Account.PerfilCliente__c,
            Account.CreatedDate
        };
    }

    public SObject obterContaPorId(String idConta)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'Id', '=', idConta);

        Map<String, Schema.SObjectField> mapaCampos = Schema.getGlobalDescribe().get('Account').getDescribe().fields.getMap();

        for(Schema.SObjectField campo : mapaCampos.values() )
        {
            if('DATE' == String.valueOf(campo.getDescribe().getType()))
                query.selectField(campo.getDescribe().getName());
        }

        for(SObject conta : (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()))
        	return conta;
        
        return null; 
    }

    public SObject obterContaPorIdChaveExterna(String idChaveExterna)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'IdChaveExterna__c', '=', idChaveExterna);

        for(SObject conta : (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()))
        	return conta;
        
        return null; 
    }

    public List<SObject> obterContasPorIdsChaveExterna(List<String> listaIdChaveExterna)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'IdChaveExterna__c', 'IN', listaIdChaveExterna);

        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL());
    }
    
    public List<SObject> obterContaPorEquipe(List<String> listaEquipe)
    {
        fflib_QueryFactory query = newQueryFactory(); 
        query.addCondition('', 'Equipe__c', 'IN', listaEquipe);
        
        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL()); 
    }
  
    public List<SObject> obterContasPorCodigosXP(List<String> codigosXP)
    {
        fflib_QueryFactory query = newQueryFactory(false, false, true); 
        query.addCondition('', 'Codigo__c', 'IN', codigosXP);
        
        return (List<SObject>) this.libDatabase.queryExecute(query.toSOQL());
    }
    
