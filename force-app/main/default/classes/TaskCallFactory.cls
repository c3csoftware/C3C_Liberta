public without sharing class TaskCallFactory 
{
    public static ITaskCallDomain novaInstanciaComCallboxChamadaViewModel(
        CallboxChamadaViewModel callboxChamadaInfo, 
        String assunto, 
        String prioridade, 
        String tipoChamada, 
        String tipo,
        String status,
        String idNomeRelacionado,
        Date dataFechamento
    )
    {
        ITaskCallDomain tarefaChamadaDomain = (ITaskCallDomain) C3C_DIContainer_Injector.newInstance('TaskCallDomain');
        TaskCallMapper tarefaChamada = tarefaChamadaDomain.obterMapper();

        tarefaChamada.idRegistro = callboxChamadaInfo.idTarefaChamada;
        tarefaChamada.assunto = assunto; 
        tarefaChamada.prioridade = prioridade; 
        tarefaChamada.tipoChamada = tipoChamada; 
        tarefaChamada.tipo = tipo;
        tarefaChamada.status = status;
        if(callboxChamadaInfo.numeroDestino.length() >= 8)
        {
            if(((Id) idNomeRelacionado).getSObjectType().getDescribe().getName() == 'Account')
                tarefaChamada.whatId = idNomeRelacionado;
            else
                tarefaChamada.idNomeRelacionado = idNomeRelacionado;
        }
        tarefaChamada.dataVencimento = dataFechamento;

        tarefaChamada.dataInicioChamada = converterTextoEmDataHora(callboxChamadaInfo.dataInicioChamada); 
        tarefaChamada.dataTerminoChamada = converterTextoEmDataHora(callboxChamadaInfo.dataTerminoChamada); 
        tarefaChamada.linkAcessarGravacaoChamada = callboxChamadaInfo.linkAcessarGravacaoChamada; 
        tarefaChamada.ramalUtilizadoChamada = callboxChamadaInfo.ramalUtilizado; 
        if(callboxChamadaInfo.numeroDestino.length() >= 8)
            tarefaChamada.referenciaChamada = callboxChamadaInfo.numeroDestino;
        tarefaChamada.duracaoSegundo = callboxChamadaInfo.duracaoSegudos;

        return tarefaChamadaDomain;
    }

    public static ITaskCallDomain novaInstanciaComIdTarefa(String idTarefa, String ramalTransferencia)
    {
        ITaskCallDomain tarefaChamadaDomain = (ITaskCallDomain) C3C_DIContainer_Injector.newInstance('TaskCallDomain');
        TaskCallMapper tarefaChamada = tarefaChamadaDomain.obterMapper();

        tarefaChamada.idRegistro = idTarefa;
        tarefaChamada.statusLigacao = 'Ligação Transferida';
        tarefaChamada.ramalTransferencia = ramalTransferencia;

        return tarefaChamadaDomain;
    }

    public static ITaskCallDomain novaInstanciaTarefaRedirecionamento(
        String idTarefa,
        String assunto, 
        String idNomeRelacionado,
        String numeroDestino,
        String ramalRedirecionamentoLigacao,
        Boolean redirecionamentoLigacao
    )
    {
        ITaskCallDomain tarefaChamadaDomain = (ITaskCallDomain) C3C_DIContainer_Injector.newInstance('TaskCallDomain');
        TaskCallMapper tarefaChamada = tarefaChamadaDomain.obterMapper();

        if(!String.isEmpty(idTarefa))
            tarefaChamada.idRegistro = idTarefa;
        tarefaChamada.assunto = assunto; 
        tarefaChamada.idNomeRelacionado = idNomeRelacionado;
        tarefaChamada.referenciaChamada = numeroDestino;
        tarefaChamada.ramalRedirecionamentoLigacao = ramalRedirecionamentoLigacao;
        tarefaChamada.redirecionamentoLigacao = redirecionamentoLigacao;
        tarefaChamada.dataVencimento = System.today();
        tarefaChamada.tipo = 'Call';

        return tarefaChamadaDomain;
    }

    //Responsável por converter texto em date e hora
    private static Datetime converterTextoEmDataHora(String dataHoraTexto)
    {
        if(dataHoraTexto == null || String.isEmpty(dataHoraTexto))
            return null;

        String dataTexto = dataHoraTexto.split(' ')[0];
        String horaTexto = dataHoraTexto.split(' ')[1];

        //Informações da data
        Integer dia = Integer.valueOf(dataTexto.split('/')[0]);
        Integer mes = Integer.valueOf(dataTexto.split('/')[1]);
        Integer ano = Integer.valueOf(dataTexto.split('/')[2]);

        //Informações da hora

        Integer hora = Integer.valueOf(horaTexto.split(':')[0]);
        Integer minuto = Integer.valueOf(horaTexto.split(':')[1]);
        Integer segundo = Integer.valueOf(horaTexto.split(':')[2]);

        return Datetime.newInstance(ano, mes, dia, hora, minuto, segundo);
    }


